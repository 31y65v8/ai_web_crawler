<script type="text/javascript">
  document.addEventListener('click', (event) => {
    var languageBox = document.getElementById("languageBox")
    var languageDrawer = document.getElementById("language-drawer")
    if (languageBox && !languageBox.contains(event.target)) {
      languageDrawer.classList.add('hidden');
    }


    var drawerLanguageBox = document.getElementById("drawer-language-box")
    var drawerLanguageDrawer = document.getElementById("drawer-language-drawer")
    if (drawerLanguageBox && !drawerLanguageBox.contains(event.target)) {
      drawerLanguageDrawer.classList.add('hidden');
    }
  })

  function showOptions() {
    var languageDrawer = document.getElementById("language-drawer")
    if(languageDrawer.classList.contains('hidden')) {
      languageDrawer.classList.remove('hidden');
    } else {
      languageDrawer.classList.add('hidden');
    }
  }

  function showDrawerOptions() {
    var languageDrawer = document.getElementById("drawer-language-drawer")
    if(languageDrawer.classList.contains('hidden')) {
      languageDrawer.classList.remove('hidden');
      var windowHeight = document.documentElement.clientHeight || document.body.clientHeight
      var langHeight = 0
      var langTop = 0
      langHeight = document.querySelector('.languageSelect').offsetHeight
      langTop = document
        .querySelector('.languageSelect')
        .getBoundingClientRect().top

      if (
        langHeight === 0 &&
        document.querySelectorAll('.languageSelect').length >= 2
      ) {
        langHeight =
          document.querySelectorAll('.languageSelect')[1].offsetHeight
        langTop = document
          .querySelectorAll('.languageSelect')[1]
          .getBoundingClientRect().top
      }

      var maxHeight = `${windowHeight - langHeight - langTop - 20}px`

      var drawerLanguageDrawer = document.getElementById("drawer-language-drawer")
      drawerLanguageDrawer.style.maxHeight = maxHeight

    } else {
      languageDrawer.classList.add('hidden');
    }
  }

  function menuCtrl() {
    var menuDrawer = document.getElementById("menu-drawer")
    if(menuDrawer.classList.contains('hidden')) {
      menuDrawer.classList.remove('hidden');
    } else {
      menuDrawer.classList.add('hidden');
    }
  }

  function getTool() {
    const currentLang = "zh"
    axiosPostRequst("/self-api/v1/tools/random", {}).then(res => {
        var code = res.data.code;
        var message = res.data.text || res.data.message || res.data.msg || "error";

        if (parseInt(code, 10) !== 200) {
            var dom = document.createElement("div");
            dom.className = "fixed top-10 left-0 right-0 flex items-center justify-center";
            var innerErrorHtml = '<div class="rounded-md bg-red-50 p-4 min-w-360" id="errorTips">'
                                    +'<div class="flex">'
                                        +'<div class="flex-shrink-0">'
                                            +'<svg class="h-5 w-5 text-red-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">'
                                                +'<path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.707 7.293a1 1 0 00-1.414 1.414L8.586 10l-1.293 1.293a1 1 0 101.414 1.414L10 11.414l1.293 1.293a1 1 0 001.414-1.414L11.414 10l1.293-1.293a1 1 0 00-1.414-1.414L10 8.586 8.707 7.293z" clip-rule="evenodd" />'
                                                    +'</svg>'
                                                +'</div>'
                                            +'<div class="ml-3">'
                                            +'<div class="text-sm font-medium text-red-800">'+message+'</div>'
                                        +'</div>'
                                    +'</div>'
                                +'</div>'
            dom.innerHTML = innerErrorHtml
            document.body.appendChild(dom);

            setTimeout(function() {
                if(document.getElementById("errorTips")) {
                    document.getElementById("errorTips").remove();
                }
            }, 3000);
        } else {
          window.location.href = `https://www.toolify.ai/${currentLang==='en' ? '' : `/${currentLang}`}/tool/${res.data.data.handle}`
        }
    });
  }

  function goBack() {
    history.back()
  }

  function toSignUp() {
    var val
    if(document.getElementById("email-address")) {
      val =  document.getElementById("email-address").value
    }
    if(val) {
      document.location.href = "https://www.toolify.ai/login?email="+val
    } else {
      document.location.href = "https://www.toolify.ai/login"
    }
  }

  function iptKeyDown() {
    var event = arguments.callee.caller.arguments[0] || window.event;
    if (event.keyCode == 13) {
      toSignUp()
    }
  }

  function toTop() {
    window.scrollTo(0,0)
  }

  window.addEventListener('DOMContentLoaded', function() {
    resetIframeH()
  });

  function resetIframeH() {
    var videoContainers = document.querySelectorAll('.editor-content iframe');

    for (var i = 0; i < videoContainers.length; i++) {
      var iframe = videoContainers[i];
      var aspectRatio = 560 / 315; // 设置视频的宽高比例

      iframe.style.height = iframe.offsetWidth / aspectRatio + 'px';
    }
  }

  if (window.addEventListener) {
    window.addEventListener("resize", function() {
      resetIframeH()
      hideNav();
    });
  } else if (window.attachEvent) {
    window.attachEvent("onresize", function() {
      resetIframeH()
      hideNav();
    });
  }

  function hideNav(){
    var languageDrawer = document.getElementById("language-drawer")
    if (languageDrawer) {
      languageDrawer.classList.add('hidden');
    }

    var drawerLanguageDrawer = document.getElementById("drawer-language-drawer")
    if (drawerLanguageDrawer) {
      drawerLanguageDrawer.classList.add('hidden');
    }

    var menuDrawer = document.getElementById("menu-drawer")
    if(menuDrawer) {
      menuDrawer.classList.add('hidden');
    }
  }


  function getScrollbarWidth() {
    var scrollDiv = document.createElement("div");
    scrollDiv.style.cssText = 'width: 99px; height: 99px; overflow: scroll; position: absolute; top: -9999px;';
    document.body.appendChild(scrollDiv);
    var scrollbarWidth = scrollDiv.offsetWidth - scrollDiv.clientWidth;
    document.body.removeChild(scrollDiv);
    return scrollbarWidth;
  }



  function hideFooterEject(){
    var footerEject = document.querySelector(".footer-eject")
    footerEject.classList.remove('showAll');

    var footerEjectTimer = setTimeout(() => {
      if(footerEject) {
        footerEject.classList.add('hide');
      }
      clearTimeout(footerEjectTimer)
    }, 1000);
  }

  document.addEventListener('DOMContentLoaded', function() {
    var warnTipsContainer = document.querySelector(".warn-tips-container")

    var warnTips = document.querySelector(".warn-tips")
    if(warnTips) {
      warnTips.classList.remove('hide-top');
    }
    var tipsTimer = setTimeout(() => {
      clearTimeout(tipsTimer)
    }, 1200);


   var footerEjectTimer = setTimeout(() => {
      var footerEject = document.querySelector(".footer-eject")
      if(footerEject) {
        footerEject.classList.add('showAll');
      }
      clearTimeout(footerEjectTimer)
    }, 8800);
  });

  const subTitles = document.querySelectorAll('.sub-title');
  subTitles.forEach((subTitle) => {
    subTitle.addEventListener('click', () => {
      const parent = subTitle.parentElement;
      if (parent) {
        parent.classList.toggle('show');
      }
    });
  });


  function throttle(fn, t) {
    let canRun = true
    return function () {
      if (!canRun) return;
      canRun = false
      setTimeout(() => {
        fn.apply(this, arguments)
        canRun = true
      }, t)
    }
  }
  function throttleImmediate(func, delay) {
    let canRun = true;

    return function() {
      if (canRun) {
        func.apply(this, arguments);
        canRun = false;

        setTimeout(function() {
          canRun = true;
        }, delay);
      }
    };
  }

  const getBroswer = function () {
    let userAgent = navigator.userAgent;
    let BrowserStr = 'Unknown'
    if (userAgent.match(/iPhone/i)) {
      BrowserStr = 'iphone'
    } else if (userAgent.match(/Android/i)) {
      BrowserStr = 'Android'
    } else if (/Chrome/.test(userAgent)) {
      BrowserStr = 'Chrome'
    } else if (/Firefox/.test(userAgent)) {
      BrowserStr = 'Firefox'
    } else if (/Safari/.test(userAgent)) {
      BrowserStr = 'Safari'
    } else if (/Edg/.test(userAgent)) {
      BrowserStr = 'Edge'
    } else if (/MSIE|Trident/.test(userAgent)) {
      BrowserStr = 'IE'
    } else if (/Opera/.test(userAgent) || /OPR/.test(userAgent)) {
      BrowserStr = 'Opera'
    }
    return BrowserStr
  }

  const getDeviceType = function () {
    if (
      /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(
        navigator.userAgent
      )
    ) {
      return 'phone'
    } else {
      return 'web'
    }
  }
  const recommendToolsDom = document.querySelectorAll('.tool-item')
  const userLanguage = navigator.language || navigator.userLanguage;
  const browserName = getBroswer()
  const deviceType = getDeviceType()
  const locale = "zh"

  for (let toolIndex = 0; toolIndex < recommendToolsDom.length; toolIndex++) {
    const toolsDom = recommendToolsDom[toolIndex]
    const hd = toolsDom.getAttribute("data-handle")
    const ad = toolsDom.getAttribute("data-ad")
    const target_url = toolsDom.getAttribute("href")

    const toolPosIndex = parseInt(toolsDom.getAttribute("data-index"))
    const toolPosition = toolsDom.getAttribute("data-position")
    const index = String(toolPosIndex + 1).padStart(2, '0')
    const toolPositionRes = toolPosition + " - " +  index

    let handlePos = function () {

      const reqData = {
        page_name: 'seo页',
        page_position: toolPositionRes,
        url: window.location.href,
        target_url: target_url,
        tool_handle: hd,
        referer:  document.referrer || null,
        page_language: locale,
        browser_language: userLanguage,
        browser: browserName,
        device_type: deviceType,
        advertisement_id: ad || null
      }
      axiosRealPostRequst("/self-api/v1/ca", reqData).then(res => {});
    }
    toolsDom.addEventListener('click', throttleImmediate(function () {
      handlePos()
    }, 2000))
  }

  document.addEventListener("DOMContentLoaded", function() {
    [].forEach.call(document.querySelectorAll('.adsbygoogle'), function(){
        (adsbygoogle = window.adsbygoogle || []).push({});
    });
  });

  let observer;
  let completedList = [];
  let waitList = [];

  function observeElements() {
    observer = new IntersectionObserver((entries) => {
      entries.forEach((entry) => {
        if (
          entry.isIntersecting &&
          entry.intersectionRatio >= 0.5 &&
          Date.now() - entry.time >= 300
        ) {
          let dataset = entry.target.dataset;
          const hd = dataset.handle;

          if (!completedList.includes(hd)) {
            const ad = dataset.ad;
            const toolPosIndex = parseInt(dataset.index)
            const toolPosition = dataset.position
            const index = String(toolPosIndex + 1).padStart(2, '0')
            const toolPositionRes = toolPosition + " - " +  index

            waitList.push({
              page_position: toolPositionRes,
              tool_handle: hd,
              advertisement_id: ad ? ad : null,
            });
            completedList.push(hd);
          }
        }
      });
    }, { threshold: 0.5, rootMargin: '0px' });

    recommendToolsDom.forEach((toolItem) => {
      observer.observe(toolItem);
    });
  }

  function handleVisibleScroll() {
    if (observer) {
      observer.disconnect();
    }

    setTimeout(() => {
      observeElements();
      setTimeout(() => {
        visibleCa();
      }, 300);
    }, 300);
  }

  function visibleCa() {
    if (waitList && waitList.length > 0) {
      var reqExposure = {
        page_name: 'seo页',
        url: window.location.href,
        referer: document.referrer || null,
        page_language: locale,
        browser_language: userLanguage,
        browser: browserName,
        device_type: deviceType,
        details: JSON.parse(JSON.stringify(waitList)),
        version: "2.4.0"
      };

      axiosRealPostRequst("/self-api/v1/ea", reqExposure);

      waitList = [];
    }
  }

  handleVisibleScroll();
  window.addEventListener('scroll', handleVisibleScroll);

  const recommendScroll = document.getElementById('recommend');
  if(recommendScroll) {
    recommendScroll.addEventListener('scroll', function() {
      handleVisibleScroll()
    });
  }

  const advertisementScroll = document.getElementById('advertisement');
  if(advertisementScroll) {
    advertisementScroll.addEventListener('scroll', function() {
      handleVisibleScroll()
    });
  }
</script>

