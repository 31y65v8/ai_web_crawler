(function () {
	var collectorURL = "https://ngds.sap.com/sites/cookies/consents";

	if (SAP.DTM.runMode !== "prod") {
		collectorURL = "https://api.services-qa.sap.com/ngds/sites/cookies/consents";
	}

	function sendToCollector(url, data, useAJAX, success_handler, error_handler) {
		console.log("[NGDS Consent]", data);
		if (!useAJAX && window.navigator.sendBeacon) {
			window.navigator.sendBeacon(url, JSON.stringify(data));
		} else {
			$.ajax({
				url: url,
				type: 'post',
				contentType: 'text/plain; charset=utf-8',
				data: JSON.stringify(data),
				success: function (data, textStatus, xhr) {
					console.log("[Data Layer]clickstream AJAX Call Status [SUCCESS]:", xhr.status);
					if (success_handler) {
						console.log("[Data Layer]clickstream AJAX callback [SUCCESS]");
						success_handler();
					}
				},
				error: function (xhr, status, err) {
					console.log("[Data Layer]clickstream AJAX Call Status [ERROR]:", xhr.status, ' with ', err);
					if (error_handler) {
						error_handler();
					}
				}
			});
		}
	}

	var OBJECT_COOKIES_REQUIRED = "Required Cookies";
	var OBJECT_COOKIES_FUNCTIONAL = "Functional Cookies";
	var OBJECT_COOKIES_ADVERTISING = "Advertising Cookies";
	var COOKIE_CONSENT_LEVELS = [OBJECT_COOKIES_REQUIRED, OBJECT_COOKIES_FUNCTIONAL, OBJECT_COOKIES_ADVERTISING];

	var FALLBACK_DOMAINS = {};
	FALLBACK_DOMAINS[OBJECT_COOKIES_REQUIRED] = ["www.sap.com"];
	FALLBACK_DOMAINS[OBJECT_COOKIES_FUNCTIONAL] = ["cai.tools.sap", "ngds.sap.com", "qualtrics.com", "liveperson.net"];
	FALLBACK_DOMAINS[OBJECT_COOKIES_ADVERTISING] = ["gigya.com"];

	function buildConsentObjectForType(cookieType) {

		var ood = localStorage.getItem("optout_domains");
		var domainConsentsPresent = ood;
		var result = {
			"general": "not configured"
		};
		var oodObject = ood ? JSON.parse(ood) : null;
		var consentsPerDomain = {};
		var somethingAllowed = false;
		var somethingDenied = false;

		if (oodObject) {
			var functionalDomainsObj = oodObject[cookieType].domains;
			for (var d in functionalDomainsObj) {
				if (functionalDomainsObj.hasOwnProperty(d)) {
					var consentStr = functionalDomainsObj[d];
					consentsPerDomain[d] = (consentStr == "1" || consentStr == "2") ? "allow" : "deny";
					if (consentStr == "1" || consentStr == "2") { somethingAllowed = true; }
					if (consentStr == "0") { somethingDenied = true; }
				}
			}
		} else {
			var level = COOKIE_CONSENT_LEVELS.indexOf(cookieType);
			var consentEnabled = _satellite.getVar('isConsentEnabled');

			FALLBACK_DOMAINS[cookieType].forEach(function (d) {
				var consent = consentEnabled(d, level);
				consentsPerDomain[d] = consent ? "allow" : "deny";
				if (consent) { somethingAllowed = true; }
				if (!consent) { somethingDenied = true; }
			});
		}

		if (somethingAllowed && somethingDenied) {
			result.general = "per-domain";
			result.domains = consentsPerDomain;
		} else if (somethingAllowed) {
			result.general = "allow";
		} else if (somethingDenied) {
			result.general = "deny";
		}

		return result;
	}


	function buildConsentData() {
		var consent_data = {
			"context": {
				"user_id": SAP.sapdx.Authentication.getUserData().userId,
				"session_id": _satellite.cookie.get('session'),
				"adobe_id": _satellite.getVisitorId().getMarketingCloudVisitorID(),
				"client_id": _satellite.cookie.get('client')
			},
			"consents": {
				"required": buildConsentObjectForType(OBJECT_COOKIES_REQUIRED),
				"functional": buildConsentObjectForType(OBJECT_COOKIES_FUNCTIONAL),
				"advertising": buildConsentObjectForType(OBJECT_COOKIES_ADVERTISING)
			}
		};

		return consent_data;
	}

	var LAST_CONSENT_CHANGE_DATETIME_KEY = "LastConsentChangeDateTime";
	var CONSENT_CHANGE_DATETIME_KEY = "referrerBeforeTrustArcReloadUpdateTime";
	var CONSENT_CHANGE_SENT_TIMES_KEY = "ConsentChangeSentTimes";
	var MAX_CONSENT_CHANGE_SENT_TIMES = 3;

	function checkConsentChange(always) {

		var lastConsentChangeDateTime = sessionStorage.getItem(LAST_CONSENT_CHANGE_DATETIME_KEY);
		var newConsentChangeDateTime = sessionStorage.getItem(CONSENT_CHANGE_DATETIME_KEY);
		var consentChangeSentTimes = parseInt(sessionStorage.getItem(CONSENT_CHANGE_SENT_TIMES_KEY)) || 0;


		if (always || (!lastConsentChangeDateTime && !newConsentChangeDateTime) ||
			(newConsentChangeDateTime && lastConsentChangeDateTime !== newConsentChangeDateTime)) {
			if (consentChangeSentTimes >= MAX_CONSENT_CHANGE_SENT_TIMES) return;

			sendToCollector(collectorURL,
				buildConsentData(),
				true,
				function () {
					sessionStorage.setItem(LAST_CONSENT_CHANGE_DATETIME_KEY, newConsentChangeDateTime);
					sessionStorage.removeItem(CONSENT_CHANGE_SENT_TIMES_KEY);
				}, function () {
					sessionStorage.setItem(CONSENT_CHANGE_SENT_TIMES_KEY, consentChangeSentTimes + 1);
				});
		}

		console.log("[NGDS Consent]", lastConsentChangeDateTime, newConsentChangeDateTime, consentChangeSentTimes);
	}

	var authModule = SAP.sapdx.Authentication;
	authModule?.subscribe({
		logic: function () {
			var OLD_LOGIN_STATUS_KEY = "oldLoginStatus";

			if (!authModule?.isAuthenticated()) {
				sessionStorage.removeItem(OLD_LOGIN_STATUS_KEY);
			} else if (authModule?.isAuthenticated() && sessionStorage.getItem(OLD_LOGIN_STATUS_KEY) !== "logY") {
				sessionStorage.setItem(OLD_LOGIN_STATUS_KEY, sessionStorage.loginStatus);
				checkConsentChange(true);
				return;
			}

			checkConsentChange(false);
		}, context: this,
		plugin: SAP.sapdx.Observer.Plugin.onlyOnce()
	});

}());
</script><script>_satellite["_runScript3"](function(event, target, Promise) {
(function() {
  const KEY = "sapcnFormDisplayed";
  
  function setFormDisplayFlag(form) {
	localStorage.setItem(KEY, JSON.stringify(form));
  }

  function removeFormDisplayFlag() {
	localStorage.removeItem(KEY);
  }

  function getFormDisplayFlag() {
    let form = localStorage.getItem(KEY);
    if (form) {
       try {
         return JSON.parse(form);
      } catch (ex) {}
    }

    return null;
  }
  
  //frame window
  $(document).on("trackData", function(e, a) {
    if (a.events.formView) {
      var form = a.form;
      setFormDisplayFlag(form);
    } else if (a.events.formSubmit) {
      removeFormDisplayFlag();
    }
  });

  //top window
  $(document).on("mfpClose", function(e, a) {
    var form = getFormDisplayFlag();
    if (form) {
      //display the chat invite using a different source
      document.dispatchEvent(createEvent('UserAbandonForm', form));
      removeFormDisplayFlag();
      if (window.triggerSCCTrackingEvent) window.triggerSCCTrackingEvent("formAbandon", "form", form);
    }
  });

})();

});</script>

