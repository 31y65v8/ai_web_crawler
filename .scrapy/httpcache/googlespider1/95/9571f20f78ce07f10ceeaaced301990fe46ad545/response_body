    $(window).on('load', function () {
        const WescrmMicroService = new window.WescrmMicroService({
            guid: "B3C789A0-9D59-43FA-1253-76B5E0EAA973",
            settingid: "68",
            formid: "955",
            siteid:  '103',
            shareurl: "https://work.wescrm.com/#/public/form/questionnaire?cardid=2024014&formid=955&fromcardid=2024014&fromguid=B3C789A0-9D59-43FA-1253-76B5E0EAA973&fromuid=17&guid=B3C789A0-9D59-43FA-1253-76B5E0EAA973&settingid=68&sharecardid=2024014&sharetime=1712128709766&sharetype=wescrm-website-visit&shareuid=17&sourceapp=qwy-admin&traceid=7181188295886376960&uid=17&styleMode=2&autoHeight=1",
        })
        // 产品体验弹窗
        WescrmMicroService.getChannelInfo({ userScopeBatch: '0' }).then(({ data }) => {
            let user = {
                realname: data.realname,
                mobile: data.mobile,
                avatar: data.avatar,
                qrcode: data.qrcode
            }
            window.localStorage.setItem('userJ', JSON.stringify(user))
        })
        // 技术交流弹窗
        WescrmMicroService.getChannelInfo({ userScopeBatch: '1' }).then(({ data }) => {
            let user = {
                realname: data.realname,
                mobile: data.mobile,
                avatar: data.avatar,
                qrcode: data.qrcode
            }
            window.localStorage.setItem('userC', JSON.stringify(user))
        })
        // 右侧导航微信扫码二维码
        WescrmMicroService.getChannelInfo({ userScopeBatch: '2' }).then(({ data }) => {
            // 侧边栏
            const rightBarImg = document.querySelector('.contact-hover img')
            rightBarImg.src = data.qrcode
        })
    })
</script>
<script>
    $(function () {
        $('.adviser-btn').click(function () {
            let userName = $(this).data('name')
            let data = window.localStorage.getItem(userName)
            data = JSON.parse(data)

            $('.adviser-popup .info-top-name').text(data.realname)
            $('.adviser-popup .tag').text('专属顾问')
            $('.adviser-popup .info-tel').attr('href', 'tel:' + data.mobile)
            $('.adviser-popup .info-tel span').text(data.mobile)
            $('.adviser-popup .avatar img').attr('src', data.avatar)
            $('.adviser-popup .tips-text').text('欢迎扫码，在线体验53AI产品')
            $('.adviser-popup .qrcode img').attr('src', data.qrcode)

            $('.adviser-popup-mask').fadeIn(300, function () {
                $('.adviser-popup').fadeIn()
            })
        })

        $('.adviser-popup-close, .adviser-popup-mask').click(function () {
            $('.adviser-popup').fadeOut(300, function () {
                $('.adviser-popup-mask').fadeOut()
            })
        })
    })
</script>
