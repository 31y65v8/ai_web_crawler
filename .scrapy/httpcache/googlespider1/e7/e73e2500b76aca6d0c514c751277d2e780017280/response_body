	$(function() {
		var videos = $("video");
		if (videos.length > 1) return;
		for (var i = 0; i < videos.length; i++) {
			videos[i].addEventListener("play", function() {
				var vs = $("video");
				for (var j = 0; j < vs.length; j++) {
					if (vs[j] !== this) {
						vs[j].pause();
					}
				}
			})
		};
	});
</script>
