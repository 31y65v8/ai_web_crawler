        function changHot(idx) {
            var ul = $('#div_hotlist').find('ul[idx=' + idx + ']');
            if (idx == '1')
                ul.next().hide();
            else
                ul.prev().hide();
            ul.show();
        }
        $(document).ready(function () {
            $('#div_hottabs a').bind("mouseenter", function () {
                changHot($(this).attr("idx"));
                $(this).parent().children().removeClass("cur");
                $(this).addClass("cur");
            });
        });
    </script>
