		<script type="text/javascript">
            var c_phone = "";
            var tm_phone = "";
			$(function() {
				$("body").on("click", function() {
					$("#select-ul").hide();
				})
				$("#select-value").on("click", function() {
					$("#select-ul").show();
					return false;
				})
				$("#select-ul>li").on("click", function() {
					var value = $(this).data("value");
					$("#select-value").text($(this).text());

					var word = $(this).attr("data-defaultsearch");
					$("#serachTxt").attr("placeholder", word);
					$("#serachTxt").val("");
					$("#hideSearchVal").val($(this).attr("data-value"));
					$("#hideSearchWord").val(word);

					$("#select-ul").hide();
					return false;
				})

				//配置导航栏样式
				// var parentUrl = getParentUrl();
				var parentUrl = null;
				if (parent !== window) {
					try {
						parentUrl = parent.location.href;
					} catch (e) {
						parentUrl = document.referrer;
					}
				}

				if (parentUrl && parentUrl.indexOf("//i.tten.cn/zffw/index.html") > -1) {
					$("#kfw_nav li").siblings().find("a").removeClass("active");
					$("#kfw_nav li:nth-child(2) a").addClass("active");
				} else if (parentUrl && parentUrl.indexOf("//i.tten.cn/news/") > -1) {
					$("#kfw_nav li").siblings().find("a").removeClass("active");
					$("#kfw_nav li:nth-child(5) a").addClass("active");
				} else if (parentUrl && parentUrl.indexOf("//www.tten.cn/trs/help/") > -1) {
					$("#kfw_nav li").siblings().find("a").removeClass("active");
					$("#kfw_nav li:nth-child(7) a").addClass("active");
				}

                $(".eyesP").click(function () {
                    if ($(this).attr("data-show") == 1) {
                        $(this).attr("data-show", "2");
                        $('#act_Infor--phone').html(c_phone);
                        $(this).prop("src", "../../images/eyeOpen.png");
                        return;
                    }
                    if ($(this).attr("data-show") == 2) {
                        $(this).attr("data-show", "1");
                        $('#act_Infor--phone').html(tm_phone);
                        $(this).prop("src", "../../images/eyeClose.png");
                        return;
                    }
                });
			})

			//function SearchChange(){
			//	var word = $("#selectType").find("option:selected").attr("data-defaultsearch");
			//	$("#serachTxt").attr("placeholder",word);
			//	$("#serachTxt").val("");
			//}

			function Search() {
				var selectVal = $("#hideSearchVal").val();
				var word = $("#hideSearchWord").val();


				var searchWord = $("#serachTxt").val() == "" ? word : $("#serachTxt").val();
				searchWord = searchWord.trim();
				var pattern = /^[\u4e00-\u9fa5_a-zA-Z0-9\s]+$/;

				var url = "http://sou.tten.cn/?keywords=" + searchWord + "&type=" + selectVal;
				url = encodeURI(url);
				window.open(url);


			}

			/*选项卡政策。*/
			function openBlank(action, data, n) {
				var form = $("<form/>").attr('action', action).attr('method', 'post');
				if (n)
					form.attr('target', '_blank');
				var input = '';
				$.each(data, function(i, n) {
					input += '<input type="hidden" name="' + i + '" value="' + n + '" />';
				});
				form.append(input).appendTo("body").css('display', 'none').submit();
			}

			//获取父页面url
			function getParentUrl() {
				var url = null;
				if (parent !== window) {
					try {
						url = parent.location.href;
					} catch (e) {
						url = document.referrer;
					}
				}
				return url;
			}
		</script>
