        function keyPress(obj) {//textarea输入长度处理
            var text = $(obj).val();
            var len; //记录剩余字符串的长度
            if (text.length >= 100){ //textarea控件不能用maxlength属性，就通过这样显示输入字符数了
                $(obj).val(text.substr(0, 100));
                len = 0;
            } else {
                len = 100 - text.length;
            }
            $(obj).next().html("最多可输入<span class='ce1'>" + len + "</span>个字");
        }
