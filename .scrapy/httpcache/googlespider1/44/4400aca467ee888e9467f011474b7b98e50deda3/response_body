        <script>
            function toSearch(){
                var element = document.getElementById("keyword");
                element.addEventListener('keyup',function (event) {
                    if( event.keyCode == 13 ){
                        toWebSite()
                    }
                })
            }
            function toWebSite(){
                document.getElementById("keyword").value='';
                window.open('https://www.hanghangcha.com/')
            }
        </script>
