  <script>
let realTY = 'marketplace';
function initScoreShow() {
    const currentUrl = location.href;
    // 配置的值是否有适配当前页面的值,没有用默认的数据
    for (let i =0; i< $('.marketplace-float-config .float-config-star').length; i++) {
        let item = $($('.marketplace-float-config .float-config-star')[i]);
        let rangeUrls = item.attr('applicationScope').split(';') || [];
        // 当前URL在配置的应用范围内
        let result = rangeUrls.filter(item => {
            let modefyItem = item;
            if (item.endsWith('/')) {
                modefyItem = item.slice(0, -1);
            }
            return currentUrl.includes(modefyItem);
        })
        if (result.length && result[0]) {
            realTY = item.attr('ty');
            // 新增栏目只有ty=mkp-new的时ty-new才有效;
            if (realTY === 'mkp_new') {
                realTY = item.attr('ty-new');
            }
            break;
        }
    }
    // 首页的话对realTY重新赋值
    if (location.pathname === '/') {
        realTY = 'marketplace_home';
    }
    // 将其他不符合条件的dom删除
    $('.marketplace-float-config .float-config-star').each(function(index, item) {
        let itemTY = $(item).attr('ty');
        if (itemTY === 'mkp_new') {
            itemTY = $(item).attr('ty-new');
        }
        if (itemTY !== realTY) {
            $(item).remove();
        }
    })

} 
  let scoreUrl = 'https://portal.huaweicloud.com/feedback/ajaxsumbit?csrf=';
 
    function initScore() {
        $(document).on('click', '.mkp-star-score-btns p', function () {
            $('input[type=checkbox]').prop('checked', false)
            $('#negative label.checked').each((index, ele) => {
                $(ele).removeClass("checked")
            })
            $('#fronts label.checked').each((index, ele) => {
                $(ele).removeClass("checked")
            })
            $('#negative input').each((index, ele) => {
                $(ele).removeAttr("disabled")
            })
            $('#fronts input').each((index, ele) => {
                $(ele).removeAttr("disabled")
            })
            let textInput = $('#textarea1 .por-textarea');
            textInput.val('');
            $('.float-config-star .por-textarea-word').text(0);
            $('.float-config-star').addClass('active');
            $('.float-config-star').css('top', '-145px');
            let $this = $(this);
            let index = $this.index();
            let textarea = $('#textarea1');
            let questionWrapper = $('.mkp-question-wrapper');
            let $negative = $('#negative');
            let $fronts = $('#fronts');
            let btnWrapper = $('.mkp-star-btn-wrapper');
            let $textDot = $('.text-required');
            $('.mkp-star-score-btns')
                .children()
                .removeClass('active');
            $('.mkp-star-score-btns')
                .children().eq(index)
                .addClass('active');
            if (textarea.is(':hidden')) {
                questionWrapper.show();
                textarea.show();
                btnWrapper.show();
            }
            if (index <= 8) {
                $negative.show();
                $fronts.hide();
                $textDot.show();
            } else {
                $negative.hide();
                $fronts.show();
                $textDot.hide();
            }
            scoreBtnStatus();
        });
        $(document).on('keyup blur', '#textarea1 .por-textarea', function () {
            scoreBtnStatus();
        });
        $(document).on('change', '.mkp-question-wrapper .por-checkbox-group', function (el) {
            let labelView = el.target.parentNode.nextElementSibling;
            if (!$(el.target).prop("checked")) {
                labelView.classList.remove("checked")
            } else {
                labelView.classList.add("checked")
            }
            if ($('#negative .por-checkbox-group').porCheckboxGroup('getVal').length >= 3) {
                $('#negative input').each((index, ele) => {
                    if (!$(ele).prop("checked")) {
                        $(ele).attr("disabled", " ");
                    }
                })
            } else {
                $('#negative input').each((index, ele) => {
                    $(ele).removeAttr("disabled")
                })
            }
 
            if ($('#fronts .por-checkbox-group').porCheckboxGroup('getVal').length >= 3) {
                $('#fronts input').each((index, ele) => {
                    if (!$(ele).prop("checked")) {
                        $(ele).attr("disabled", " ");
                    }
                })
            } else {
                $('#fronts input').each((index, ele) => {
                    $(ele).removeAttr("disabled")
                })
            }
            scoreBtnStatus();
        });
 
        $(document).on('click', '.btn-submit', function () {
            let $submitbtn = $('.mkp-star-btn-wrapper .btn-submit');
            let $tips = $('.float-config-star-tips');
            if (!$submitbtn.attr('disabled')) {
                $submitbtn.attr('disabled', '');
                let score = $('.mkp-star-score-btns .active').index() || 0;
                if (score < 0 || score > 10) {
                    return;
                }
                let help = score > 6 ? 5 : 1;
                let currentHref = window.location.href;
                let column = realTY;
                let reason = $('#textarea1 .por-textarea').val() || '';
                let chooseProblem = (score <= 8 ? $('#negative .por-checkbox-group'): $('#fronts .por-checkbox-group')).porCheckboxGroup('getVal');
                let serviceId = '';
                let contactId = '';
                let idDomain = 'https://portal.huaweicloud.com';
                let idUrl =
                    idDomain +
                    '/rest/cbc/portalapppublishservice/v1/api/developer/service/id?location=www.huaweicloud.com/common/score-serviceid-edit.html&usePage=' +
                    location.href +
                    '&filterCode=111';
 
                $.ajax({
                    method: 'GET',
                    url: idUrl,
                    success: function (data) {
                        serviceId = data && data.data && data.data.serviceid ? data.data.serviceid : '';
                        contactId = data && data.data && data.data.contactid ? data.data.contactid : '';
                        let starData = {
                            problemCategory: base64Encode(chooseProblem.join('、')),
                            fb: base64Encode(reason),
                            ih: help,
                            ty: column,
                            ti: base64Encode($(document).attr('title') || '对该页面的满意度评价？'),
                            sc: score,
                            url: currentHref,
                            productcode: serviceId,
                            contactId: contactId,
                            pp: '',
                        };
                        if (column.length > 0) {
                            $('head').prepend('<meta name="referrer" content="no-referrer-when-downgrade">');
                            $.ajax({
                                method: 'POST',
                                url: scoreUrl,
                                async: true,
                                dataType: 'json',
                                data: starData,
                                crossDomain: true,
                                xhrFields: {withCredentials: true},
                                success: function (data) {
                                    if (data.data.result == 1) {
                                        starSuccess($submitbtn, $tips);
                                        // 埋点数据上报
                                        if (column === 'marketplace_home') { // 首页/搜索页面
                                            window.sendData('marketplace_home', '首页/搜索页', starData);
                                        } else if (column === 'marketplace_detail') { // 详情页
                                            window.sendData('marketplace_detail', '详情页', starData);
                                        } else if (column === 'marketplace_zone') { // 专区页
                                            window.sendData('marketplace_zone', '专区页', starData);
                                        } else if (column === 'marketplace_activity')  { // 活动页
                                            window.sendData('marketplace_activity', '活动页', starData);
                                        } else { // 默认首页/搜索页面
                                        window.sendData('marketplace_home', '首页/搜索页', starData);
                                        }
                                    } else {
                                        starFail($submitbtn, $tips);
                                    }
                                },
                                error: function (err) {
                                    starFail($submitbtn, $tips);
                                },
                            });
                        } else {
                            starFail($submitbtn, $tips);
                        }
                    },
                    error: function (err) {
                    },
                });
            }
        });
        $(document).on('click', '.mkp-star-btn-wrapper .btn-cancel', function () {
            $('.marketplace-float-config').removeClass('show-score');
            $('.float-config-star').addClass('hide');
            $('.float-config-star').css('top', '0');
            setTimeout(function () {
                $('.float-config-star').removeClass('hide');
                resetFeedback();
            }, 1e3);
        });
        $(document).on('click', '.tip-close-icon', function () {
            localStorage.setItem("isCloseTime", new Date().getTime());
            $('.marketplace-float-config').removeClass('show-score');
            $('.float-config-star').addClass('hide');
            $('.feedback').removeClass('hover');
            setTimeout(function () {
                $('.float-config-star').removeClass('hide');
                resetFeedback();
            }, 1e3);
        });
        $('.float-config-star .por-textarea').on('input', function () {
            $('.float-config-star .por-textarea-word').text($(this).val().length);
        });
 
        function starSuccess($submitbtn, $tips) {
            $tips.removeAttr('style');
            $submitbtn.removeAttr('disabled');
            $('.marketplace-float-config').removeClass('show-score');
            $('.float-config-star').addClass('hide');
            let noticeOpt = {content: '提交成功', type: 'success', container: '.float-config-star-tips'};
            InfoNotice(noticeOpt);
            setTimeout(function () {
                resetFeedback();
                $('.por-tip-notice-outContainer').remove();
                $tips.css("display", "none");
            }, 1200);
            setTimeout(function () {
                $('.float-config-star').removeClass('hide');
            }, 2e3);
        }
 
        function starFail($submitbtn, $tips) {
            $tips.removeAttr('style');
            $submitbtn.removeAttr('disabled');
            $('.marketplace-float-config').removeClass('show-score');
            $('.float-config-star').addClass('hide');
            let noticeOpt = {content: '提交失败', type: 'error', container: '.float-config-star-tips'};
            InfoNotice(noticeOpt);
            setTimeout(function () {
                $('.por-tip-notice-outContainer').remove();
                $tips.css("display", "none");
            }, 1200);
            setTimeout(function () {
                $('.float-config-star').removeClass('hide');
            }, 2e3);
        }
 
        function base64Encode(input) {
            let _keyStr = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=';
 
            function _utf8_encode(string) {
                string = string.replace(/\r\n/g, '\n');
                let utftext = '';
                for (let n = 0; n < string.length; n++) {
                    let c = string.charCodeAt(n);
                    if (c < 128) {
                        utftext += String.fromCharCode(c);
                    } else if (c > 127 && c < 2048) {
                        utftext += String.fromCharCode((c >> 6) | 192);
                        utftext += String.fromCharCode((c & 63) | 128);
                    } else {
                        utftext += String.fromCharCode((c >> 12) | 224);
                        utftext += String.fromCharCode(((c >> 6) & 63) | 128);
                        utftext += String.fromCharCode((c & 63) | 128);
                    }
                }
                return utftext;
            }
 
            let output = '';
            try {
                let chr1, chr2, chr3, enc1, enc2, enc3, enc4;
                let i = 0;
                input = _utf8_encode(input);
                while (i < input.length) {
                    chr1 = input.charCodeAt(i++);
                    chr2 = input.charCodeAt(i++);
                    chr3 = input.charCodeAt(i++);
                    enc1 = chr1 >> 2;
                    enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);
                    enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);
                    enc4 = chr3 & 63;
                    if (isNaN(chr2)) {
                        enc3 = enc4 = 64;
                    } else if (isNaN(chr3)) {
                        enc4 = 64;
                    }
                    output = output + _keyStr.charAt(enc1) + _keyStr.charAt(enc2) + _keyStr.charAt(enc3) + _keyStr.charAt(enc4);
                }
            } catch (e) {
                output = '';
            }
            return output;
        }
 
        function resetFeedback() {
            $('.float-config-star').removeAttr('style');
            $('.mkp-star-score-btns p').removeClass('active');
            $('#textarea1 .por-textarea').val('');
            $('#textarea1').hide();
            $('.mkp-question-wrapper .por-checkbox-group input').removeAttr('checked');
            $('.mkp-question-wrapper').hide();
            $('.mkp-star-btn-wrapper').hide();
            $('.float-config-star').removeClass('active');
        }
    }
 
    function InfoNotice(opt) {
        console.log("InfoNotice")
        if (opt && opt.content) {
            let _targetContainer$find;
            let option = opt;
            option.type = option.type || 'success';
            let targetContainer = $(option.container || '#content' || 'body');
            let template = '<div>\n        <div class=" por-tip-notice '
                .concat(
                    option.type,
                    '">\n                            <div class="por-tip-notice-content" style="width:120px;">\n                                <div class="por-tip-notice-text">\n                                    <span>'
                )
                .concat(
                    option.content,
                    '</span>\n                                </div>\n                            </div>\n                        </div>\n                     </div>'
                );
            let infoEle = $(template);
            let tipName = '.por-tip-notice-outContainer';
            let removeTime = 1200;
            if (!((_targetContainer$find = targetContainer.find(tipName)) === null || _targetContainer$find === void 0 ? void 0 : _targetContainer$find.length)) {
                targetContainer.prepend(infoEle);
            } else {
                targetContainer.find(tipName).each(function (_i, v) {
                    $(v).remove();
                });
                targetContainer.prepend(infoEle);
            }
            setTimeout(function () {
                infoEle.remove();
            }, removeTime);
        }
    }
 
 
    function scoreBtnStatus() {
        let star = $('.mkp-star-score-btns').find('p.active').index();
        let $submitbtn = $('.mkp-star-btn-wrapper .btn-submit');
        let textInput = $('#textarea1 .por-textarea');
        let length = $('#textarea1 .por-textarea').val().trim().length;
        let negativeChoose = $('#negative .por-checkbox-group').porCheckboxGroup('getVal');
        let frontsChoose = $('#fronts .por-checkbox-group').porCheckboxGroup('getVal');
 
 
        if ((star <= 8 && textInput.val() !== '' && length <= 200 && negativeChoose.length !== 0) || (star > 8 && frontsChoose.length !== 0 && length <= 200)) {
            $submitbtn.removeAttr('disabled');
        } else {
            $submitbtn.attr('disabled', '');
        }
    }
 
    function getCustomerCookie(c_name) {
        let c_start = '';
        let c_end = '';
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + '=');
            if (c_start !== -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(';', c_start);
                if (c_end === -1) c_end = document.cookie.length;
                return decodeURI(document.cookie.substring(c_start, c_end));
            }
        }
        return '';
    }
 
    function initAiShow() {
        // 不是IE浏览器走白名单展示逻辑
        let ua = navigator && navigator.userAgent;
        if (!isIE() && !(ua.toLowerCase().indexOf('android') > -1 || (/iPad/i).test(ua) || (/iPhone/i).test(ua))) {
            let sessionID = getCustomerCookie("SessionID")
            if (sessionID) {
                let entryWhiteListStr = $('#ai-shop-app').attr('data-entry-white-list');
                if(entryWhiteListStr !== ''){
                    let entryWhiteList = entryWhiteListStr.split(',');
                    for (let item in entryWhiteList) {
                        if (sessionID.endsWith(entryWhiteList[item])) {
                            $('#marketplace-ai-outer').removeClass("hide-my-self")
                            $('#slidebar-line').removeClass("hide-my-self")
                        }
                    }
                }
            }
        } else {
            $('#marketplace-ai-outer').addClass("hide-my-self")
            $('#slidebar-line').addClass("hide-my-self")
        }
    }
 
    function isIE() {
        let ua = window.navigator.userAgent;
        let msie = ua.indexOf('MSIE ');
        let trident = ua.indexOf('Trident/');
 
        return (msie > 0 || trident > 0);
    }
 
    initScoreShow();
    initScore();
    initAiShow();
    // 滚动时，最下方的回到页面顶部的图标
    const win_height = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
    const pageHeight = $(document).height();
    // 年毫秒值
    const year = 31622400;
    $(window).scroll(function (event) {
        let currTop = $(window).scrollTop();
        if (currTop > win_height) {
            $('.marketplace-sidebar').addClass('show-totop');
        } else {
            $('.marketplace-sidebar').removeClass('show-totop');
        }
        if (currTop / pageHeight >= 0.5 && (!localStorage.getItem("isCloseTime") || ((new Date().getTime() - localStorage.getItem("isCloseTime")) / 1000 > year))) {
            $('.marketplace-float-config').addClass('show-score');
        } else {
            $('.marketplace-float-config').removeClass('show-score');
        }
 
    });
    
</script>
</div>

</div>
    

