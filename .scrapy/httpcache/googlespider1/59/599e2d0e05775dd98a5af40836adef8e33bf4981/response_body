<script>

    // 用.html设置dom时，对dom中的变量进行转义，防止xss攻击
    function htmlEscape(str) { //字符转义

        if (typeof str === 'number') {
            return str;
        }
        var escapesMap = {
            '<': '&lt;',
            '>': '&gt;',
            '"': '&quot;',
            "'": '&#039;',
            '&': '&amp;',
            '/': '&#x2F;'
        };

        var reUnescapedHtml = new RegExp(/[<>"'&/]/g);
        return (str && reUnescapedHtml.test(str)) ? str.replace(reUnescapedHtml, function (chr) {
            return escapesMap[chr];
        }) : (str || "");
    }

    window.htmlEscape = htmlEscape;

    // cookie 获取数据
    function getUaCookie(c_name) {
        if (document.cookie.length > 0) {
            var c_start = document.cookie.indexOf(c_name + '=');
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                var c_end = document.cookie.indexOf(';', c_start);
                if (c_end == -1) c_end = document.cookie.length;
                return decodeURI(document.cookie.substring(c_start, c_end))
            }
        }
        return ''
    }

    // 创建axios实例
    const service = axios.create({
        timeout: 60000, // 请求超时时间
        withCredentials: true,
    });
    window.interceptorsRequest = service;

    // 请求拦截器
    service.interceptors.request.use(
        config => {
        if (config.url.indexOf('mkpdata') !== -1) {
            let csrfToken = getUaCookie('csrf-token');
            if (csrfToken) {  
                config.headers['csrf'] = csrfToken;
            }
        }
        return config;
        },
        error => {
        return Promise.reject(error);
        },
    );

    $.fn.scrollUnique = function() {
        return $(this).each(function() {
            var eventType = "mousewheel";
            if (document.mozHidden !== undefined) {
                eventType = "DOMMouseScroll"
            }
            $(this).on(eventType, function(event) {
                var scrollTop = this.scrollTop
                , scrollHeight = this.scrollHeight
                , height = this.clientHeight;
                if ("wheelDeltaX"in event.originalEvent && event.originalEvent.wheelDeltaX !== 0)
                    return;
                if ("deltaX"in event.originalEvent && event.originalEvent.deltaX !== 0)
                    return;
                if ("axis"in event.originalEvent && event.originalEvent.axis === event.originalEvent.HORIZONTAL_AXIS)
                    return;
                var delta = event.originalEvent.wheelDelta ? event.originalEvent.wheelDelta : -(event.originalEvent.detail || 0);
                delta = delta / 5;
                if (delta > 0 && scrollTop <= delta || delta < 0 && scrollHeight - height - scrollTop <= -1 * delta) {
                    this.scrollTop = delta > 0 ? 0 : scrollHeight;
                    event.preventDefault()
                }
            })
        })
    }

</script>


<script>

    // 埋点SDK初始化
     _hasdk = window.ugasdk;
     if (_hasdk) {
      try {
        // 设置SDK上报地址
        if (location.host.includes('marketplace.ulanqab')) {
            _hasdk.setEnv('preprod');
        } else if (location.host === 'marketplace.huaweicloud.com') {
            _hasdk.setEnv('prod');
        } else {
            _hasdk.setEnv('preprod');
        }
        _hasdk.initSDK();
        // 设置站点标识
        _hasdk.setAppId('marketplace');

      } catch (error) {
        
      }
     }

   // 封装上报函数
    function sendData(eventId, eventName, contentObj) {
      if (!_hasdk) {
        return;
      }
      const domainId = window.domainId;
      if (domainId) {
        _hasdk.setUid(domainId);
        contentObj['domainId'] = domainId;
      }
      const userId = window.$bbs_userID;
      if (userId) {
        contentObj['bbs_userId'] = userId;
      }
      _hasdk.sendData(eventId, eventName, contentObj);
    }
    
    /**
    * 用户对用户可控参数进行javascript编码
    * @param sHtml 初始数据
    * @return 编码后的数据
    */
    function js2Escape(sHtml) {
        return sHtml.replace(/[&<>"':\[\]$()%+\/\\#`*,\-;=^|]/g, function(c) {
            return {
                '&': '\\x26',
                '<': '\\x3c',
                '>': '\\x3e',
                '"': '\\x22',
                '\'': '\\x2;',
                ':': '\\x3a',
                '[': '\\x5b',
                ']': '\\x5d',
                '$': '\\x24',
                '(': '\\x28',
                ')': '\\x29',
                '%': '\\x25',
                '+': '\\x2b',
                '\\': '\\x5c',
                '/': '\\x2f',
                '#': '\\x23',
                '`': '\\x60',
                '*': '\\x2a',
                ',': '\\x2c',
                '-': '\\x2d',
                ';': '\\x3b',
                '=': '\\x3d',
                '^': '\\x5e',
                '|': '\\x7c'
            }[c]
        })
    }

    //用户对用户可控参数进行url编码
    function url2Escape(sHtml) {
        return sHtml.replace(/[&<>"':\[\]$()%+\/\\#`*,\-;=^|]/g, function(c) {
            return {
                '&': '%26',
                '<': '%3c',
                '>': '%3e',
                '"': '%22',
                '\'': '%27',
                ':': '%3a',
                '[': '%5b',
                ']': '%5d',
                '$': '%24',
                '(': '%28',
                ')': '%29',
                '%': '%25',
                '+': '%2b',
                '\\': '%5c',
                '/': '%2f',
                '#': '%23',
                '`': '%60',
                '*': '%2a',
                ',': '%2c',
                '-': '%2d',
                ';': '%3b',
                '=': '%3d',
                '^': '%5e',
                '|': '%7c'
            }[c]
        })
    }

    //用户对用户可控参数进行css编码
    function css2Escape(sHtml) {
        return sHtml.replace(/[&<>"':\[\]$()%+\/\\#`*,\-;=^|]/g, function(c) {
            return {
                '&': '\\26',
                '<': '\\3c',
                '>': '\\3e',
                '"': '\\22',
                '\'': '\\27',
                ':': '\\3a',
                '[': '\\5b',
                ']': '\\5d',
                '$': '\\24',
                '(': '\\28',
                ')': '\\29',
                '%': '\\25',
                '+': '\\2b',
                '\\': '\\5c',
                '/': '\\2f',
                '#': '\\23',
                '`': '\\60',
                '*': '\\2a',
                ',': '\\2c',
                '-': '\\2d',
                ';': '\\3b',
                '=': '\\3d',
                '^': '\\5e',
                '|': '\\7c'
            }[c]
        })
    }
    
    //  检查路径是否http或https开头
    function httpCheck(input) {
        if (!input) {
            return;
        }
        const reg = new RegExp(/^(http|https).*$/);
        if (!reg.test(input)) {
            return "https://" + input;
        }
        return input;
    }

</script>
