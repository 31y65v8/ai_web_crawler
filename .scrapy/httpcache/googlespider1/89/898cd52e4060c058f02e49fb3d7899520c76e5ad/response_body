实战",92716,{"author":447,"authorId":6,"categories":448,"centerADid":6,"centerAuthorId":452,"chapterUpdatetime":10,"cover":453,"discount":6,"extra":454,"finished":56,"free":37,"hasEpub":56,"id":456,"intro":457,"isTxt":5,"lastChapter":458,"lastChapterName":10,"price":459,"score":6,"sexAttr":64,"title":460,"totalChapters":458,"totalWords":461,"type":37},"张大斌 田恒义 许桂秋主编",[449,450,451],{"id":42,"name":43,"shortName":43},{"id":45,"name":46,"shortName":47},{"id":49,"name":50,"shortName":50},"30704292704722801","http://ccstatic-1252317822.file.myqcloud.com/bookcoverimg/2024-09-23/8a2864afbc64d4ce428e34a7564a99e2.jpg",{"favcount":63,"allwords":63,"newchaptertime":63,"staticscore":455},"25499",51892895,"本书是一本全面介绍人工智能概念和新发展、新兴技术、产业应用和安全伦理思考的通识类图书，分为认知篇、前沿篇、行业篇、思考篇四部分，从人工智能的发展历程、模型等基础知识讲起，通过智能机器人、自动驾驶、大模型、AI生成等热门应用让读者对人工智能技术有直观的认识和理解，并从多个领域对人工智能存在问题进行了深度分析。本书内容既有人工智能技术的深度，也有行业应用的广度，对人工智能技术进行全面剖析。本书适合对人工智能感兴趣的读者使用，也适合零基础读者学习人工智能概念性、原理性内容使用。",70,3880,"人工智能导论（通识版）",101831,[463],"http://ccstatic-1252317822.file.myqcloud.com/epubpublic/8B7B42/29427161304485106/OEBPS/Styles/stylesheets.css","1.2.1 三起两落","2024-05-23 17:06:17",3236,-789339388,{},{},{"$i9bCpVDU6M":63,"req1":63,"req2":63,"req3":63},{"site":472},{"isLogin":5,"initData":7,"userInfo":473,"globalReportData":474,"curUrlQrcode":10,"supportWebp":5},{"iconurl":10,"nick":10,"prefer":10,"userCoins":6,"vipType":6,"userTickets":6,"firstOpen":5,"memberType":6,"freeType":6,"smsType":6,"content":10,"desc":10},{"pn":475},"read-bid-cid"]</script><script>window.__NUXT__={};window.__NUXT__.config={public:{NUXT_ENV:"pro",NUXT_SITE:"chuangshi",NUXT_PLATFORM:"m",FL:"https://imgservices-1252317822.image.myqcloud.com/coco/s10192022/cefc2a5d.pz1phw.js",FH:"https://imgservices-1252317822.image.myqcloud.com/coco/s10192022/5e8e7db5.crn08q.js",SITE_CONSTANTS:{siteName:"创世中文网",appId:1450000219,areaId:7,defaultChannelId:{cn1:200003965,cn2:200003966,cn3:200003967,cn4:200003968,cn5:200003969,cn6:200003970,cn7:200003971,cn8:200003972,cn9:200003973,cn10:200