<script>window.addEventListener('DOMContentLoaded', function() {



(function($){
    const debounce = (f, delay)=>{
        let timeoutId;
        return () => {
            if(timeoutId){
                clearTimeout(timeoutId);
            }
            timeoutId = setTimeout(f, delay);
        }
    }    


    $(document).ready(function(){
        const body = $(document.body);
        const searchWrap = $('#dood-mobile-search-wrap');
        const searchButton = $('#td-top-mobile-search-toggle');
        const menuWrap = $('#td-mobile-nav');
        const menuButton = $('#td-top-mobile-toggle');
        
        const mobileSearchBox = $('#dood-mobile-search-wrap #mobile-search');
        const mobileSearchCloseButton = $('#dood-mobile-search-wrap #mobile-search .mobile-search-close');
        const mobileSearchSubmit =  $('#dood-mobile-search-wrap #mobile-search .mobile-search-submit');
        const mobileSearchInput =  $('#dood-mobile-search-wrap #mobile-search .mobile-search-input input');
        const mobileSearchCategoryFilter = $('.mobile-search-filter-category');
        const mobileSearchOrderByFilter = $('.mobile-search-filter-orderby');
        const mobileSearchContent = $('.mobile-search-content');
        const mobileSearchResult = $('.mobile-search-results');

        const toggleSearchBox = () => body.toggleClass('mobile-search-open');
        const closeSearchBox = () => body.removeClass('mobile-search-open');
        const closeMenuBox = () => body.removeClass('td-menu-mob-open-menu');

        searchButton.click(()=>{ jQuery(document).scrollTop(0); closeMenuBox(); toggleSearchBox();  });
        menuButton.click(closeSearchBox);

        mobileSearchCloseButton.click(closeSearchBox);

        const search = () => {
            const keyword = mobileSearchInput.val();
            const cat = mobileSearchCategoryFilter.val();
            const orderBy = mobileSearchOrderByFilter.val();
            
            if( !keyword ) {
                return;
            }

            const limit = 6;

            const paintResult = (content) => {
                mobileSearchResult.html(content);
                mobileSearchResult.fadeOut(0);
                mobileSearchResult.fadeIn('slow');
            }

            paintResult('Searching ...');

            mobileSearchContent.pagination({
                dataSource: function(done) {
                            $.ajax({
                                type: 'POST',
                                url: '/wp-admin/admin-ajax.php',
                                data: { action: 'data_fetch', keyword , cat, orderBy, limit},
                                success: function(response) {
                                    const res = $.parseJSON(response);
                                    const totalPages = Math.ceil(res.total_results / limit)
                                    const data = new Array(totalPages);

                                    data[0] = res.data;
                                    $('.paginationjs').remove();
                                    done(data);
                            }
                        });
                },
                pageSize: 1,
                callback: function(data, pagination) {
                    const { pageNumber } = pagination;
                    let content = data;

                    if(pageNumber !== 1){
                        paintResult('Loading...');
                        $.ajax({
                                type: 'POST',
                                url: '/wp-admin/admin-ajax.php',
                                data: { action: 'data_fetch', keyword , cat, orderBy, page: pageNumber, limit },
                                success: function(response) {
                                    content = $.parseJSON(response).data;
                                    paintResult(content);
                            }
                        });
                    } else {
                        paintResult(content);
                    }
                }
            })
        }

        mobileSearchInput.on('input', debounce(search, 500));
        mobileSearchCategoryFilter.change(search);
        mobileSearchOrderByFilter.change(search);
        // mobileSearchSubmit.click(search);

        mobileSearchCategoryFilter.change(() => {
            console.log('Change');
            const optionSelected = mobileSearchCategoryFilter.find(":selected");
            optionSelected.attr("selected","selected");
        });

        mobileSearchSubmit.click(() => {
            const optionSelected = mobileSearchCategoryFilter.find(":selected");
            const cat = optionSelected.attr('data-category');

            if (cat == 'china-business-law-journal-zh-hans') window.location.href = '/zh-hans/?s=&category_name='+cat+'&orderby=post_date&submit=&lang=zh-hans';
            else window.location.href = '/?s=&category_name='+cat+'&orderby=post_date&submit=&lang=en';
            return false;
       });
    });
})(jQuery);

});</script>

