<script type="text/javascript">
var _hmt = _hmt || [];
(function() {
  var hm = document.createElement("script");
  hm.src = "//hm.baidu.com/hm.js?f1b9aef44b8656063724e8564c0924d0";
  var s = document.getElementsByTagName("script")[0];
  s.parentNode.insertBefore(hm, s);
})();
$(".footContainer p span").addClass("zhandian");

$(function(){
	$('.swiper-slide').width($(window).width())
    $('.swiper-wrapper').width(($('.swiper-slide').width())*($('.swiper-slide').length+2));
	$(window).resize(function(){
		$('.swiper-slide').width($(window).width())
    	$('.swiper-wrapper').width(($('.swiper-slide').width())*($('.swiper-slide').length+2));
	})
    function swiperlunbo(){
        $('.swiper-wrapper').stop().animate(
       		{marginLeft:-($('.swiper-slide').width())},
       		500,
       		function(){
				$('.swiper-wrapper').css('margin-left','0px');
       			$('.swiper-wrapper div:first').remove().clone(true).appendTo('.swiper-wrapper');
			})
    };
	//暂时关闭轮转
	var swipertimer=setInterval(swiperlunbo,4000)
    $('.swiper-button-next').click(function(){
		clearInterval(swipertimer);
        swiperlunbo()
		clearInterval(swipertimer);
    })
    $('.swiper-button-prev').click(function(){
		clearInterval(swipertimer);
        $('.swiper-wrapper').css('margin-left',-($('.swiper-slide').width()));
		$('.swiper-wrapper').animate({marginLeft:0},500);
        $('.swiper-wrapper div:last').remove().clone(true).prependTo('.swiper-wrapper');
		clearInterval(swipertimer);
    })
	$('.swiper-button-prev,.swiper-button-next').mouseout(function(){
		clearInterval(swipertimer);
		swipertimer=setInterval(swiperlunbo,4000)
	})
})
</script>
