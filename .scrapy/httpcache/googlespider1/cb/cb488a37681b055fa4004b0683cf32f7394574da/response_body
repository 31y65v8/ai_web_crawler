<script>

    var  url=location.href;

    $.ajax({

        type : "get",

        url : "http://www.cn156.com/jssdk.php?url="+url,

        dataType : "jsonp",

        jsonp: "callback",

        jsonpCallback:"success_jsonpCallback",

        success : function(data){

            wx.config({

                debug: false,

                appId: data.appId,

                timestamp: data.timestamp,

                nonceStr: data.nonceStr,

                signature: data.signature,

                jsApiList: [

                    'onMenuShareTimeline',//

                    'onMenuShareAppMessage',

                    'onMenuShareQQ',

                    'onMenuShareWeibo',

                    'onMenuShareQZone'

                ]

            });

        },

        error:function(data){

          alert(data);
          //  alert("连接失败！");

        }

    });

    wx.ready(function () {

        var shareData = {

            title: '人工智能行业人才需求旺盛',

            desc: '未来5到10年，对社会改变最大的将是人工智能（AI）。人工智能将深刻改变社会，代替很多岗位，也会创造出很多新的岗位。哪个国家掌握了人工智能的话语权，就掌握了全球的话语权。',

            link: url,

            imgUrl: 'http://www.cn156.com/assets/addons/cms/img/noimage.jpg'

        };

        wx.onMenuShareAppMessage(shareData);//分享给好友

        wx.onMenuShareTimeline(shareData);//分享到朋友圈

        wx.onMenuShareQQ(shareData);//分享给手机QQ

        wx.onMenuShareWeibo(shareData);//分享腾讯微博

        wx.onMenuShareQZone(shareData);//分享到QQ空间

    });

    wx.error(function (res) {
