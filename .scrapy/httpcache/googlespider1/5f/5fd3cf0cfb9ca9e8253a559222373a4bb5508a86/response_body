function setCookieValueNew(cname, cvalue, expdays) {
var d = new Date();
expdays = expdays || 1;
d.setTime(d.getTime() + (expdays * 24 * 60 * 60 * 1000));
var expires = "expires=" + d.toUTCString();
document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}
