<script>
    $(window).scroll(function () {
        if ($(this).scrollTop() > 500) {
            $(".moblie,.retu,.retu_box").fadeIn('200')
        } else {
            $(".moblie,.retu,.retu_box").fadeOut('100')
        }
    });
    var newtop;
    $('.retus').on('click', function () {
        var timer = setInterval(function () {
            newtop = $(document).scrollTop();
            if ($(document).scrollTop() == 0) {
                clearInterval(timer);
            } else {
                var a = $(document).scrollTop(newtop - 40);
            }
        }, 5);
    })
    $('.pushClose').click(function(){
        $('.pushBox').hide()
        sessionStorage.truePush = true;
    })

    if(sessionStorage.trunClose){
        $('.count-down').hide();
        $(".countdown").hide()
    }
    $(document).ready(function(){
        $(".wrong,.wrong_fix").click(function () {
            $('.count-down').hide();
            $(".countdown").hide()
            sessionStorage.trunClose = true;
        })

        function getQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            if (r != null)
                return unescape(r[2]);
            return null;
        }
    })
    function checkRem() {
        if($(window).width()<960){
            resizeRem(375)
        }
    }
    checkRem()
    window.onresize = function () {
        checkRem()
    }
    var _url =  document.URL;
    $(".k_nav_list>li").each(function(){
        var _href = $(this).find('a').attr('href');
        if(_href && _href.length > 10)
        if(_url.indexOf(_href) > -1){
            $(this).addClass('active').siblings().removeClass('active');

        }
    })
    $('.k_listIcon').click(function () {
        if($('.k_nav_list').is(':hidden')){
            $('.k_nav_list').show(300)
        }else{
            $('.k_nav_list').hide(300)
        }

    })
    if((navigator.userAgent.match(/(phone|pad|pod|iPhone|iPod|ios|iPad|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i))) {
        var a = 1
        $('.k_navBox .k_nav .k_nav_list li').click(function () {
            if(a){
                $(this).find('ul').show()
                a = 0
            }else{
                $(this).find('ul').hide()
                a = 1
            }
        })
    }
</script>
