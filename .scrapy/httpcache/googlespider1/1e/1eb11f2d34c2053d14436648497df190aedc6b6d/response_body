<script src="//static.wicongress.org.cn/clearmin/js/jquery.qrcodejs.min.js"></script>
<script>
    $(document).ready(function(){
        $(document).on('click','.collect',function () {
            var _uid = $(this).attr('data-uid');
            var _aid = $(this).attr('data-aid');
            if(_uid && _aid){
                $.ajax({
                    url:"//www.wicongress.org.cn/account/collect",
                    type:'post',
                    dataType:'json',
                    data:{uid:_uid,aid:_aid,type:'collect'},
                    success:function (res) {
                        if(res.code =10000){
                            $(".shouchang img").attr('src',res.iurl);
                            $(".shouchang span").text(res.text);
                            $(".shouchang").addClass('uncollect').removeClass('collect');
                        }
                    }
                })
                return false;
            }
        })
        $(document).on('click','.uncollect',function(){
            layer.confirm('您确定取消收藏吗？',{btn:['确定','取消'],title:false},function(index){
                var _uid = $(".uncollect").attr('data-uid');
                var _aid = $('.uncollect').attr('data-aid');
                if(_uid && _aid){
                    $.ajax({
                        url:"//www.wicongress.org.cn/account/collect",
                        type:'post',
                        dataType:'json',
                        data:{uid:_uid,aid:_aid,type:'uncollect'},
                        success:function (res) {
                            if(res.code =10000){
                                $(".shouchang img").attr('src',res.iurl);
                                $(".shouchang span").text(res.text);
                                $(".shouchang").addClass('collect').removeClass('uncollect');
                            }
                        }
                    })
                    layer.close(index);
                    return false;
                }
            })
        });
        $(document).on('click','.unreg',function(){
            $(".reg_captache_li").remove();
            $("#loginphone").val('');
            $("#logincode").val('');
            $(".login_box").show();
        })
    })
    var qr_url = document.URL;
    var _qrcode  = new QRCode("shareArt",{ text:qr_url,width:120,height:120})
</script>
