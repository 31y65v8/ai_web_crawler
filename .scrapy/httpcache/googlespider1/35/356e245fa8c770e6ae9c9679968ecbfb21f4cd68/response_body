			window.loginFlag = false
		});
		
		$(".group input[name=userid]").focus(function(){
			$(".errorId").hide();
		});
		
		$(".group input[name=password]").focus(function(){
			$(".errorPass").hide();
		});
		
		//登录提交表单
		$(".login_btn").click(function(){
			var userId = $(".group input[name=userid]").val();
			var password = $(".group input[name=password]").val();
			userId = $.trim(userId);
			password = $.trim(password);
			
			if(userId == ""){
				$(".errorId").show();
				return ;	
			}else{
				$(".errorId").hide();
			};
			
			if(password == ""){
				$(".errorPass").html("请输入密码").show();
				return ;	
			}else{
				$(".errorPass").hide();
			};
			var data = {
				keyword:userId,
				password:password
			};
			var usertype = $('#usertype').val();
			loginMember(usertype,data,'');
		});
	});
</script><div class="gl-footer"><div id="g-footer" class="g-footer"><div class="g-wrap"><div class="g-footer-nav"><a target="_blank" href="//www.ofweek.com/about.shtml" rel="nofollow">关于我们</a> -
