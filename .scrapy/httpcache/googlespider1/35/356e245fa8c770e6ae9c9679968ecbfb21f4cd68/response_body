	$(function(){
		//文章举报弹窗关闭
		$(".turnoff,.cancel").click(function(){
			$(this).parents(".m-report").hide();
		});
		//举报的选项选择
		$(".comment_type").click(function(){
			$(".m-report textarea").hide();
			$(".m-report textarea").val('');
		});


		//我要吐槽
		$("#comment_other").click(function(){
			$("input[name=report_type]:checked").attr('checked',false);
			$(".m-report textarea").toggle();
		});

		//举报按钮
		$(".m-user_comment").on('click','.report',function(e){
			var val = $(this).attr('data-id');
			$("#comment_confirm").attr('data-id',val);
			$("#report_comment").show();
			e.preventDefault();
		});

		//举报确认按钮
		$("#comment_confirm").click(function(e){
				var type = $("input[name=report_type]:checked").val();
				var content = $.trim($("textarea[name=reportcontent]").val());
				if(type == undefined && content.length == 0){
				errorPop('请选择其中一项');
				return false;
			}
			//其他对应的是-1
			if(content){
				type = -1;
			}
			var id = $(this).attr('data-id');
			var url = '/comment/ajax_report';
			var data = {id:id,report_type:type,content:content};
			$.post(url,data,function(data){
				errorPop(data.info);
				if(data.status == 1){
					$('#report_comment').hide();
				}
			},'json');
		});
	})
