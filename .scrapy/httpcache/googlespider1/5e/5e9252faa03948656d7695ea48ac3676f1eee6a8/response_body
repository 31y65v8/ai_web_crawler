<script>
    function setEdnOpenLink() {
    let artId = $("meta[name='art_id']").attr("content") || null;
    let subId = $("meta[name='sub_id']").attr("content") || null;
        if (artId !== null && subId !== null) {
            let url = new URL(window.location.href);
            let protocol = url.protocol;
            let host = url.host;
            // host 轉正式環境
            host = 'money.udn.com'
            let concatUrl = protocol + '//' + host +'/money/story/' + subId + '/' + artId
            //修改 link
            let ednOpenLink = `https://edn.page.link/?link=${concatUrl}&isi=517585333&ibi=com.udn.edn.plus&apn=com.udn.econdailyplus`
            $('.open-on-app__link').attr('href',ednOpenLink)
        } else {
            $('.open-on-app__button').css('display','none');
        }
    }

    function showOpenOnAppBtnCheck() {
        let cookieName = 'UnShowOenpBtn';
        let allCookies = document.cookie;
        if (allCookies.indexOf(cookieName) !== -1) {
            $('.open-on-app__button').css('display','none');
        }
    }

    setEdnOpenLink();
    showOpenOnAppBtnCheck()

    //關閉動作
    $('.open-on-app__button i').click((e)=>{
        e.preventDefault();
        $('.open-on-app__button').css('display','none');
        // 設置cookie 當天00點
        let todayEndTime = new Date();
        todayEndTime.setHours(23, 59, 59 ,99);
        let expires = "expires=" + todayEndTime.toGMTString();
        document.cookie = "UnShowOenpBtn=true" + "; " + expires + '; domain=udn.com; path=/';
    });
</script>						  <noscript>
