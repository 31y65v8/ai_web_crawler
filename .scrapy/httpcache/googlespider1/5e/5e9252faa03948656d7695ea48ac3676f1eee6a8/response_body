window.addEventListener('message', function(event) {
    var eventData = event.data;
    if (eventData.event === 'start' || eventData.event === 'play' || eventData.event === 'pause'|| eventData.event === 'progress'|| eventData.event === 'complete') {
        window.dataLayer.push({
            'event': eventData.event,
