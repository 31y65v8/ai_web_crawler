<!-- #Location: temp_new/inc/component/activity_notice -->          <script>
  // 排除登入後去除廣告 導致googlettag的console error
  
//  ad 21019 有開放
$(document).ready(function(){
    let bHeight = $(document.body).height();
    let bWidth = $(document.body).width();
    let showPopupDistance = 50;
    let inStory = false;
    // 內文頁設定 popup距離
    if (window.location.href.indexOf('/money/story/') > -1) {
      //取得到id=story_end 的位置 把showPopupDistance設定成該位置
      showPopupDistance = $('#story_end').offset().top;
    } else {
      //非內文頁設定 popup距離
      if (bWidth <= 450) {
        showPopupDistance = (bHeight / 7);
      }
    }
    //活動的notice1-1
    let now = new Date();
    let endDay = new Date(countDown());
    if ( document.cookie.indexOf("SeenActivityNotice=") == -1 && now < endDay && document.cookie.indexOf("SeenActivityCloseNotice=") == -1 && !popupShowed) {

      $(window).on('scroll', function() {
        let scrollDistance =  $(window).scrollTop();
            if (scrollDistance > showPopupDistance && document.cookie.indexOf("SeenActivityNotice=") == -1 && document.cookie.indexOf("SeenActivityCloseNotice=") == -1 && !popupShowed) {
              //沒有popup的時候不上mask

              setTimeout(() => {
                $('.noticeBox--activity').css('display','block');
                if($('.noticeBox--activity .noticeBox__img').length > 0) { 
                  $('.notice-mask').css('display','block');
                }
              }, 1000);
            }
      });
    }

  //活動的notice1-2 noticeBox__btn--close survey 不再提醒
  $('.noticeBox--activity .noticeBox__btn--close').on('click', function(){
          $('.noticeBox--activity').css('display','none');
          $('.notice-mask').css('display','none');
          setActivityCloseCookie()
    });

    //活動的notice1-2 noticeBox__btn--skip survey 明天提醒
    $('.noticeBox--activity .noticeBox__btn--confirm').on('click', function(){
        $('.noticeBox--activity').css('display','none');
        $('.notice-mask').css('display','none');
        setActivityCookie()
    });

    //添加點擊mask 關閉的功能 記得針對正上映的popup 處理
    $('.notice-mask').on('click', function(){
        $('.noticeBox--activity').css('display','none');
        $('.notice-mask').css('display','none');
        setActivityCookie()
    });

    //設定12 AM 6點過期的cookie
    function setActivityCookie() {
        // 12點過期
        var judgeClock = new Date();
        judgeClock.setHours(23, 59, 59 ,99);
        judgeClock.setTime(judgeClock.getTime() + ( 6 * 60 * 60 * 1000)) // 原本時間 + 6個小時 am 05:59:59
        // console.log('tTime = ' + judgeClock.getTime());
        var expires = "expires=" + judgeClock.toGMTString();
        // console.log(expires);
        document.cookie = "SeenActivityNotice=true" + "; " + expires + '; domain=udn.com; path=/';
    }

    function setActivityCloseCookie() {
        // 12點過期
        var judgeClock = new Date(countDown()); //下線時間
        judgeClock.setTime(judgeClock.getTime()) // 原本時間
        // console.log('tTime = ' + judgeClock.getTime());
        var expires = "expires=" + judgeClock.toGMTString();
        // console.log(expires);
        document.cookie = "SeenActivityCloseNotice=true" + "; " + expires + '; domain=udn.com; path=/';
    }
    
    //問券頁面填寫完畢 自動添加cookie
    if (window.location.href.indexOf('?20220321_survey_completed') > -1 ) {
        setActivityCloseCookie();
    }

});
</script>    
