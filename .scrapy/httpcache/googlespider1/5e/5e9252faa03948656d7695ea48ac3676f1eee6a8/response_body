<script>
$(document).ready(function() {
    if ($(".article-related .more1").length < 5) $(".story__read-more--related").css("visibility","hidden");
    if ($(".article-hot .more1").length < 5) $(".story__read-more--hot").css("visibility","hidden");

    $('.story__read-more').on('click', function(e) {
      var obj = this;
      showmoreList(e,obj);
    });

    function showmoreList(e,obj) {
      e.preventDefault();
      var morePage = $(obj).attr("data-page");
      // console.log(morePage);
      morePage = parseInt(morePage) + parseInt(1);
      $(".more" + morePage).each(function() {
				$(this).css("display","flex");
			});
      $(".story__read-more").each(function() {
        $(this).attr("data-page", morePage )  ; 
			});
      var next_morePage = parseInt(morePage)+parseInt(1);
      if ($(".article-related .more"+next_morePage).length == 0) $(".story__read-more--related").css("visibility","hidden");
      if ($(".article-hot .more"+next_morePage).length == 0) $(".story__read-more--hot").css("visibility","hidden");
    }

  });
	$(window).on("load resize",function(e) {

		/* 手機板：左、右的圖繞文，轉為居中 */
		var newWindowWidth = $(document).width();
		if ( newWindowWidth < 768 ) {
			$('#story_body').find('div').each(function(i, ojb) {
				if ( $(this).hasClass('photo_left') ) {
					$(this).addClass('photo_center_m');
				} else if ( $(this).hasClass('photo_right') ) {
					$(this).addClass('photo_center_m');
				}
			});
		}
	});
	$(function(){
		moreId = parseInt($("#moreId").attr("value"))+parseInt(1);
		if ($(".more" + moreId).length == 0)
		{
			$(".showmore").css("display","none");
		}
		$(".article-keyword__list a").on("click",function(){
			$.ajax({
        url: "https://misc.udn.com/api/EventCollect"+$(this).attr("rel"),
				cache: false,
				success : function (response) {}
			});
		});
		/*
		if( $("#pv").length ){
			$.ajax({
				url: $("#pv").attr("rel"),
				cache: false,		
				success: function(response) {}
			});
			console.log(1);
		}*/
		// $.ajax({ url: $("#pv").attr("rel"), cache: false  });
		$.ajax({ url: $("#ppv").attr("rel"), cache: false, xhrFields: {withCredentials: true}  });
	});
</script>
