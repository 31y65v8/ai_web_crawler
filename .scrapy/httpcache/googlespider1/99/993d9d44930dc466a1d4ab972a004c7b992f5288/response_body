<script> 
    function parseQueryString(url) {
        var result = {};
        var str = url.split("?")[1];
        if (str == undefined)
            return result;
        var items = str.split("&")

        var arr;
        for (var i = 0; i < items.length; i++) {
            arr = items[i].split("=");
            result[arr[0]] = arr[1];
        }
        return result;
    };

    function initPage() {
        var params = parseQueryString(window.location.href);
        var version = params.version;
        if(version === 'slh'){
            document.getElementById('headerAnchor').innerHTML = pcSlhHeaderStr;
            document.getElementById('footerAnchor').innerHTML = pcSlhFooterStr;
            $(".pos").css("font-size", '26px');
            $(".pos").css("margin-bottom", '30px');
            $(".pos").css("margin-top", '30px');
            $(".ly").css("margin-top", '12px');
            $(".ly").css("margin-bottom", '12px');
            $(".introduce").addClass("wjnr");
            $(".con").css("margin-bottom", '30px');
            $(".explain").css("font-size", '32px');
            $(".zw-title").css("font-size", '40px');
            $(".zw-info").css("font-size", '26px');
            $(".zw-info").addClass("flexFixed");
            $(".zw").css("font-size", '32px');
            $(".viewList").addClass("intro");
            $(".aside").addClass("xgjd");
            $(".pos a").each(function (index) {
    const regex = /(http:\/\/www.gd.gov.cn)(.*)/gm;
    let m, group1, group2;

    let a_Url = $(this).attr('href');

    while ((m = regex.exec(a_Url)) !== null) {
        if (m.index === regex.lastIndex) {
            regex.lastIndex++;
        }

        m.forEach(function (match, groupIndex) {
                if (groupIndex == 1) {
                    group1 = match;
                } else if (groupIndex == 2) {
                    group2 = match;
                }
            }
        );
        
    }

    if(index < 2){
        var reg = RegExp(/\/zzllms/);
        if(a_Url.match(reg)){
            $(this).attr("href", group1 + group2);
        }else{
            var text=$(this).text();
            if(text=="页尾"){
                $(this).attr("href", group1 + group2);
            }else{
                $(this).attr("href", group1 + '/zzllms' + group2);
            }
            
        }
    }
});
$('a').click(function (e){
    e.preventDefault();
    var temp = parseQueryString($(this).attr("href"));
    var url = $(this).attr("href");
    var reg = RegExp(/gd\.gov\.cn/gm);
    if($(this).attr("href").match(reg)){
            if(JSON.stringify(temp) === '{}'){
                if($(this).closest('.page').length > 0){
                    window.open($(this).attr("href") + "?version=slh","_self");
                }else{
                    window.open($(this).attr("href") + "?version=slh");
                }
            }else{
                window.open($(this).attr("href") + "&version=slh");
            }
        }else{
            if(!url||url == 0){
                
            }else{
                if((url.indexOf("gd.gov.cn")>-1 || $.trim(url).substr(0,2).indexOf("./")>-1  || $.trim(url).indexOf("cloudtest-ducha.southcn.com")>-1||$.trim(url).indexOf("gov.cn")>-1|| $.trim(url).indexOf("#")>-1 || $.trim(url).indexOf("javascript:")>-1)){ 
                    window.open(url,"_blank");
                    return true;
                }else{
                    checkUrl(url);
                    return false;
                }
            }
        }
    
});

        }else{
            document.getElementById('headerAnchor').innerHTML = pcHeaderStr;
            document.getElementById('footerAnchor').innerHTML = pcFooterStr;
        }
    }

    initPage();
</script>
