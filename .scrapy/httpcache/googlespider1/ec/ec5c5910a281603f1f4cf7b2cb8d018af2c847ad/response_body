<script type="text/javascript">

    $(function(){

     var userid=null;

          $.ajax({

            type:'GET',

            url:'https://www.enicn.com/api.php?op=user',

            async:false,

            dataType:'json',

            success:function(msg){

              if(msg.islogin){

                var objuserid=msg.userid;

                if(objuserid>0){

                  userid=objuserid;

                }

              }

            }

          });

        var _opForm = function(op) { $(".fromSubit").find('input,button').prop('disabled', op); };

        $("#flie_download").click(function(){

            var url=$("#flie_download_url").val();

            if(userid==null){

                alert("请先登录！");

                var currenturl = window.location.href;

                window.location="https://www.enicn.com/index.php?m=member&c=index&a=mobile_login&forward="+currenturl;

            }else{

              window.location.href=url;

            }

            

        })

    });

    </script>

