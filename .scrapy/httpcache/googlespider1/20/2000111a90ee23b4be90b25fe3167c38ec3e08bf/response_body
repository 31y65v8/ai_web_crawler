<script>
	function getQueryString(name) {
		var reg = "/(\\?|\\&)" + name + "=([^\\&]+)/";
		reg = eval(reg);
		var r = window.location.href.substr(1).match(reg);
		if (r != null) {
			return unescape(r[2]);
		}
		return null;
	}
	if(getQueryString('access_from') && getQueryString('access_from')=='qr'){
		$('.pc_scan_shadow').show()
	}
	$('.pc_scan_shadow_btn').click(function(){
		$('.pc_scan_shadow').hide()
	})	
</script>
<script>
	$(function(){
		// 点击认证文章 
		$(".readAll").on("click",function(){
			//console.log("认证文章");
			if(nowmemToken){
				var url = window.location.href;
				location.href='https://www.cn-healthcare.com/z/speciallist/auth/auth.html?backUrl='+encodeURIComponent(url);
			}else{
				pubshowmsg("登录后才能操作",'',"login(1)");
				return;
			}
		});

		// 健康号跳转
		$("#authorInfo,#author-photowap").on('click',function(){
			var memCard = $("#curmemCard").val();
			location.href='https://ucenter.cn-healthcare.com/dynamic/other/index?memCard='+memCard;
		});
	})
</script>

