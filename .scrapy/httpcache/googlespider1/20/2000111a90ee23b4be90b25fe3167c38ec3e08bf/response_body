<script>
    function setFooterQRcode(){ 	// 设置底部二维码效果
        $('.wx-collect-box .RQcode-item-link').mouseover(function(){
            var _index = $(this).index() + 1;
            $(this).find('.wx-QRcode-img').show();
        }).mouseout(function(){
            $('.wx-QRcode-img').hide();
        })

        $('.share-wx-box .RQcode-item-link').mouseover(function(){
            var _index = $(this).index() + 1;
            $(this).find('.wx-QRcode-img').show();
        }).mouseout(function(){
            $('.wx-QRcode-img').hide();
        })

        $('.popular-products .pro-item').mouseover(function(){
            var _index = $(this).index() + 1;
            $(this).find('.pro-code-img').show();
        }).mouseout(function(){
            $('.pro-code-img').hide();
        })
    }
    setFooterQRcode();
</script>
