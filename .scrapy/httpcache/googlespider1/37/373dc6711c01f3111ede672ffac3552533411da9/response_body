	   function SearchHeaderForm(){
		   $("#search_header").submit()
		   }function SearchIndexForm(){
			   $("#search_index").submit()
		}function selectProduct(e){
			$("#search-box").val(e),
			$("#suggesstion-box").hide()
		}
		</script>
			 
<script type="text/javascript">
