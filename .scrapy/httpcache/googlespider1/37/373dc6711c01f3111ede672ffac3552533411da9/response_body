function hover(element) {
  element.setAttribute('src', 'https://www.fortunebusinessinsights.com/zh/img/zhimg/Buy-Now-White_new.svg');
}
function unhover(element) {
 element.setAttribute('src', 'https://www.fortunebusinessinsights.com/zh/img/zhimg/Buy-Now-Green.svg');
}
function hoverpb(element) {
  element.setAttribute('src', 'https://www.fortunebusinessinsights.com/zh/img/zhimg/Pre-book-White.svg');
}
function unhoverpb(element) {
 element.setAttribute('src', 'https://www.fortunebusinessinsights.com/zh/img/zhimg/Pre-book-Green-01.svg');
}
$('#m_submit').click(function(){
		var red={"border-color": "red","box-shadow": "0 0 0 0.2rem hsla(0, 100%, 48%, 0.25)"};
		var none={'border-color':'#ced4da','box-shadow':'none'};
		var m_email=$('#m_email').val();
		var m_message=$('#m_message').val();
		var m_name=$('#m_name').val();
		var m_slug=$('#m_slug').val();
		var m_title=$('#m_title').val();
		var m_url=$('#m_url').val();
		var m_user_email=$('#m_user_email').val();
		var m_data=[m_slug,m_title,m_url];
		
		var reg = /^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
		if(m_email.trim() == '' ){
			$('#m_email').css(red).focus();
			return false;
		}else{
			$('#m_email').css(none);
		}
		if(m_user_email.trim() == '' ){
			$('#m_user_email').css(red).focus();
			return false;
		}else {
			$('#m_user_email').css(none);
		}
		if(m_user_email.trim() != '' && !reg.test(m_user_email)){
			$('#m_user_email').css(red).focus();
			return false;
		}else {
			$('#m_user_email').css(none);
		}	
		report_mail(m_email, m_message, m_name,m_data,m_user_email);
	})
	
	
function report_mail(m_email, m_message, m_name,m_data,m_user_email)
 {
	var m_submit='1';
	$.ajax({
	url:"https://www.fortunebusinessinsights.com/report_mail.php",
	method:"POST",
	data:{m_email:m_email,m_message:m_message,m_name:m_name,m_data:m_data,m_user_email:m_user_email, m_submit:m_submit},
	cache:false,
	beforeSend: function() {
		$('#m_submit').prop('disabled', true); 
	},
   success:function(data)
   {
	   console.log(data);
	   if(data=='Done'){
		   //$('#exampleModal').modal('hide');
		   $('#m_submit').prop('disabled', false); 
		   $("#m_email").val('');
		   $("#m_user_email").val('');
		   $("#m_name").val('');
		   $("#sharedmailsuccess").text('You have successfully Share this report!');		   
		   setTimeout(function(){ $("#sharedmailsuccess").text(''); },5000);
		   //Swal.fire( 'Thank You!', 'You have successfully Share this report!', 'success');
	   }
   }
  });
 }
