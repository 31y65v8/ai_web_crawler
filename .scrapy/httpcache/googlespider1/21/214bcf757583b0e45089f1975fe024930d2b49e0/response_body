                        <div class="inner-20">
                            <div class="about-tit">
                                <h2> 专家顾问</h2>
                            </div>
                            <div class="expert-list">
                                <ul class="fixed">
                                    
 <li class="wow fadeInDown" data-wow-duration="0.5s" data-wow-delay="0s" style="height: 370px;">
                                        <div class="imgshow">
                                            <img src="/upload/image/20211124/20211124161922_81101.png" alt="bingsheng he" title="bingsheng he" class="ratio-img" data-ratio="1" style="height: 120px;">
                                        </div>
                                        <div class="text">
