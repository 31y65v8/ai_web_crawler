<script>
        // document.addEventListener('click',e => console.log(e.target));
        $('.js_toggle_tv_line').on('click', function (event) {
            if ($(this).hasClass('active')) {
                //     закрытие
                if ($('.js_live_window').hasClass('active')) {
                    $('.js_live_window').removeClass('active');
                    $('.js_column_padding').removeClass('active');
                    $('.js_start_live').removeClass('active');

                }
            } else {
                //     открытие
                $('.js_live_window').addClass('active');
                $('.js_column_padding').addClass('active');
                $('.js_start_live').addClass('active');
            }



            $(this).toggleClass('active');
            $(this).closest('.tv_line').toggleClass('active');

            if ($(window).width() < 767) {
                $('body, html').toggleClass('hidden')
            }

            const iframe = document.querySelector('#video-iframe');
            const iframeSrc = iframe.dataset.src;
            iframe.innerHTML = `<iframe class="iframe" allowfullscreen="" src="${iframeSrc}"></iframe>`;

            setTimeout(function() {
                $('#video-iframe iframe').click();
            }, 200);
        })
    </script><script>
    $(function () {
        $(document).on('submit', '#subscribe-form-in-detail', function (e) {
            e.preventDefault();
            let $form = $(this),
                $formWrap = $('#subscribe_form_block'),
                $errorMsgBlock = $(this).find('.error_message'),
                $submitBtn = $form.find('[type="submit"]');

            if ($submitBtn.length) {
                $submitBtn.attr('disabled', true);
            }

            $errorMsgBlock.html('');

            $.ajax({
                url: $form.attr('action'),
                data: new FormData($form.get(0)),
                type: 'POST',
                dataType: 'json',
                processData: false,
                contentType: false,
                success: function (data) {
                    if ($submitBtn.length)
                        $submitBtn.attr('disabled', false);
                    if (data.STATUS === 'ERROR') {
                        $errorMsgBlock.html(data.MESSAGE)
                    } else if (data.STATUS === 'SUCCESS') {
                        $formWrap.html(data.MESSAGE);
                        $formWrap.addClass('active');
                        $('.mailing__input').val('');
                        setTimeout(function () {
                            $formWrap.html('');
                            $formWrap.removeClass('active');
                        }, 3000)
                    }
                },
            });
        });
    });
</script><script>
        $(function() {
            function SetReaction(item, lang, iblock, vote) {
                $.ajax({
                    url: '/ajax/reaction.php',
                    data: {
                        'item': '797675',
                        'lang': 'cn',
                        'iblock': '63',
                        'vote': vote,
                    },
                    type: 'POST',
                    success: function(data) {
                        $('#reaction_block').html(data);
                    },
                });
            }

            SetReaction('797675', 'cn', '63', '');

            $(document).on('click', '.js_like', function(e) {
                e.preventDefault();
                SetReaction('797675', 'cn', '63', 1);
            });
        });
    </script>
