<script>
    $(function(){
        $(document).on('submit', '#subscribe-form', function(e) {
            e.preventDefault();
            let $form = $(this),
                $formWrap = $('#subscribe_form_block'),
                $errorMsgBlock = $(this).find('.error_message'),
                $submitBtn = $form.find('[type="submit"]');

            if ($submitBtn.length) {
                $submitBtn.attr('disabled', true);
            }

            $errorMsgBlock.html('');

            $.ajax({
                url: $form.attr('action'),
                data: new FormData($form.get(0)),
                type: 'POST',
                dataType: 'json',
                processData: false,
                contentType: false,
                success: function(data) {
                    console.log(data)
                    if ($submitBtn.length)
                        $submitBtn.attr('disabled', false);
                    if (data.STATUS === 'ERROR') {
                        $errorMsgBlock.html(data.MESSAGE)
                    } else if(data.STATUS === 'SUCCESS') {
                        $formWrap.html(data.MESSAGE);
                        $formWrap.addClass('active');
                        $('.mailing__input').val('');
                        setTimeout(function () {
                            $formWrap.html('');
                            $formWrap.removeClass('active');
                        }, 3000)
                    }
                },
            });
        });
    });
