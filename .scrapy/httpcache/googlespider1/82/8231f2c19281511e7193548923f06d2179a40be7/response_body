<script type="text/javascript">
	// <![CDATA[

		

		Liferay.currentURL = '\x2fzh\x2fweb\x2ftechnology-trends\x2fartificial-intelligence\x2fask-the-experts\x2ftechtrends-ai-firth';
		Liferay.currentURLEncoded = '\x252Fzh\x252Fweb\x252Ftechnology-trends\x252Fartificial-intelligence\x252Fask-the-experts\x252Ftechtrends-ai-firth';

