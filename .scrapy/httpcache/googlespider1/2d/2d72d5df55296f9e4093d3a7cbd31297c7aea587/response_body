    <script type="text/javascript">
        $(function () {
            // ======微信分享 START======
            // var _jsapiServer = "https://mi4ms.amonsoft.com"
            // var _jsapiServer = "https://mi4ms.amonsoft.com"
            var _jsapiServer = " ";
            var apiUrl = _jsapiServer + "/mi4-web/oauth/getJsapiTicket";
            var appId = "?appId=wx1bfc16aced66417f";//微信APPID
            var pageUrl = window.location.href;
            var urlParam = "&url=" + pageUrl;
            var pTitle = '中国将始终是世界发展的重要机遇——写在第七届中国国际进口博览会开幕之际'; //
            var pDesc = '国纪平   中国共产党二十届三中全会擘画了进一步全面深化改革、推进中国式现代化的宏伟蓝图，进一步全面深化改革，要坚持改革和开放相统一。';
            var pLink = window.location.href;
            var pCover = 'http://www.dangjian.cn/static/imgae/1905.jpg';

            // if(pCover){
            //     pCover =pCover.replace('https','http');
            //     console.log(pCover);
            // }



            // if (pCover.indexOf('http') == -1) {
            //     pCover = '/mi4-website/template-yozee/img/sharelogo.jpg';
            // }

            // 加载评论内容
            loadMi4Fragment(apiUrl + appId + urlParam, function (result) {
                // 正常处理
                console.log(result, 'result');
                if (!result.config) return;
                wx.config({
                    debug: false, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印。
                    appId: result.config.appId, // 必填，公众号的唯一标识
                    timestamp: result.config.timestamp, // 必填，生成签名的时间戳
                    nonceStr: result.config.nonceStr, // 必填，生成签名的随机串
                    signature: result.config.signature,// 必填，签名，见附录1
                    jsApiList: [
                        'checkJsApi',
                        'updateAppMessageShareData',
                        'updateTimelineShareData',
                        'onMenuShareTimeline',
                        'onMenuShareAppMessage'
                    ] // 必填，需要使用的JS接口列表，所有JS接口列表见附录2
                });
                // 分享给好友
                // alert(pCover);

                wx.ready(function () {

                    wx.updateAppMessageShareData({
                        title: pTitle, // 分享标题      
                        desc: pDesc, // 分享描述
                        link: pLink, // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
                        imgUrl: String(pCover), // 分享图标
                        success: function () {
                            // alert(pDesc)
                            // alert('分享成功');
                        }
                    });
                    wx.updateTimelineShareData({
                        title: pTitle, // 分享标题
                        link: pLink, // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
                        imgUrl: pCover, // 分享图标
                        success: function () {
                            // alert('分享朋友圈成功');
                        }
                    });
                    wx.onMenuShareTimeline({
                        title: pTitle, // 分享标题
                        link: pLink, // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
                        imgUrl: pCover, // 分享图标
                        success: function () {
                            // alert('分享朋友圈成功(即将废弃)');
                        }
                    });
                    wx.onMenuShareAppMessage({
                        title: pTitle, // 分享标题
                        desc: pDesc, // 分享描述
                        link: pLink, // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
                        imgUrl: pCover, // 分享图标
                        type: 'link', // 分享类型,music、video或link，不填默认为link
                        dataUrl: '', // 如果type是music或video，则要提供数据链接，默认为空
                        success: function () {
                            alert('!!')
                            // alert('分享朋友成功(即将废弃)');
                        }
                    });
                    wx.error(function (res) {
                        console.log("errorMSG:", res);
                    });
                    wx.checkJsApi({
                        jsApiList: ['updateTimelineShareData', 'onMenuShareAppMessage', 'onMenuShareTimeline', 'onMenuShareAppMessage'], // 需要检测的JS接口列表，所有JS接口列表见附录2,
                        success: function (res) {
                            console.log('checkJsApi:', res);
                        }
                    });
                });

            }, function (e) {
                console.log(e);
            });
            // ======微信分享 END======
        })
    </script>


