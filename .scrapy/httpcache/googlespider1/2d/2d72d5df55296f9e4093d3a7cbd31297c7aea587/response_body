            success: function (data) {

                $("#noenav").html(data);
            }
        })
        $.ajax({
            url: '/footer.html',
            type: "get",
            success: function (data) {

                $("#footer").html(data);
            }
        })

        /*放置视频*/
        $(".qplayer_item").css("display", "none")
        var value = $(".qplayer_item").attr("src");
        if (value) {
            // 当DOM准备就绪时初始化video.js播放器
            document.addEventListener('DOMContentLoaded', function () {
                var videoht = " <video id='example_video_1' class='video-js vjs-default-skin' controls preload='auto'>+ <source src='' type = 'video/mp4'/>+</video>";
                $(".video-sj").append(videoht);
                $("#example_video_1 source").attr("src", value);
                document.getElementById("example_video_1").style.display = "block";

                var player = VideoJS(document.getElementById('example_video_1'), {
                    poster: '',

                })

            });
        } else {

            $(".video-sj").css("display", "none");
            document.getElementById("example_video_1").style.display = "none";
            $(".video-sj").addClass("heade")
            $(".video-js").addClass("heade")
            $("#example_video_1").addClass("heade")
        }


    </script>


