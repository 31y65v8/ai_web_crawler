<script type="text/javascript">
    (function () {
        var sidebarTitle = $(".layout-left-nav h2");
        var areaTitle = $(".menu-title .title h2");
        var navList = $(".header .menu li");


        $.each(navList, function () {
            var $this = $(this);
            var menuText = $.trim($this.text());

            if(
                (sidebarTitle.length && menuText == $.trim(sidebarTitle.text())) 
                || (sidebarTitle.length && menuText == '辟谣' && $.trim(sidebarTitle.text()) == '科学辟谣')
            ) {
                $this.find("a").addClass("active");
            }
        });

        $(document)
            .on('mouseenter mouseleave', '._mainNavMore', function (e) {
                var $list = $(this).find('._moreList');

                if (e.type == 'mouseenter') {
                    $list.show();
                } else {
                    $list.hide();
                }
            });
    }());
</script>
