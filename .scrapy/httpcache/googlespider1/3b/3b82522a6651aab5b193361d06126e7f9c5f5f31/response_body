<script type="text/javascript">
    function getAbsoluteUrl(url) {
        var img = new Image();
        img.src = url; // 设置相对路径给Image, 此时会发送出请求
        url = img.src; // 此时相对路径已经变成绝对路径
        img.src = null; // 取消请求
        return url;
    }
    var has_img = $(".TRS_Editor").find("img");
    var trs_img = "https://img3.kepuchina.cn/images/2018/shareH5.png";
    if(has_img.length > 0) {
        trs_img = getAbsoluteUrl($(".TRS_Editor img:first").attr("src"));
    }
    if (!window.wxShare) {
      $.ajax({
          url: "https://backend.kepuchina.cn/gettoken",
          dataType: "jsonp",
          type: "get",
          data: {
              "url": document.location.href.split('#')[0]
          },
          success: function(res) {
              wx.config({
                  debug: false,
                  appId: res.appId,
                  timestamp: res.timestamp,
                  nonceStr: res.noncestr,
                  signature: res.signature,
                  jsApiList: ['updateAppMessageShareData', 'updateTimelineShareData']
              });
              wx.ready(function() {
  
                  wx.updateTimelineShareData({
                      desc:"科普中国网_让科技知识在网上和生活中流行",
                      title: document.title, // 分享标题
                      imgUrl: trs_img, // 分享图标
                      link: document.location.href.split('#')[0], // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
                      success: function() {
                          // 用户确认分享后执行的回调函数
                      },
                      cancel: function() {
                          // 用户取消分享后执行的回调函数
                      }
                  });
                  wx.updateAppMessageShareData({
                      desc:"科普中国网_让科技知识在网上和生活中流行",
                      title: document.title, // 分享标题
                      imgUrl: trs_img, // 分享图标
                      link: document.location.href.split('#')[0], // 分享链接，该链接域名或路径必须与当前页面对应的公众号JS安全域名一致
                      success: function() {
                          // 用户确认分享后执行的回调函数
                      },
                      cancel: function() {
                          // 用户取消分享后执行的回调函数
                      }
                  });
              });
              wx.error(function(err) {});
          },
          error: function(res) {}
      });
    }
