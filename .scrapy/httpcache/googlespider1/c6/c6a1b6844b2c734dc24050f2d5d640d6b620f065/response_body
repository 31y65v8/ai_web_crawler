            var nav_id = 2;
            function searchSite(url) {
                if($("#searchKey").val()=="输入关键字进行搜索") {
                    alert("请输入关键字!");
                    return;
                }
                /*var wbEncode= getCookie("targetEncoding" + cookieDomain.replace(/\./g,""));
                if ( wbEncode == 1) {
                    $("#searchKey").val(translateText($("#searchKey").val()));
                }*/
                $("#searchForm").attr("action",url);
                $("#searchForm").submit();
            }
            function dosearch() {
                if($("#searchKey").val()=="输入关键字进行搜索" || $("#searchKey").val()=="") {
                    alert("请输入关键字!");
                    return false;
                }
                return true;
            }
