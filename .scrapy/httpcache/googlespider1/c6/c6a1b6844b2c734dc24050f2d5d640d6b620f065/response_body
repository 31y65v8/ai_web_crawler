<script type="text/javascript">
  var defaultEncoding = "2";
  var translateDelay = "50";
  var cookieDomain = "";
  var msgToTraditionalChinese = "繁w中文";
  var msgToSimplifiedChinese = "简体中文";
  var translateButtonId = "GB_BIG";

  $(function () {
    $('span#_span_jiucuo').css('padding-top', '0');
    $(".bdsharebuttonbox").hover(function () {
      $(".bdshare_popup_box").css("z-index", 99998);
    });
    $("#_ideConac img").attr("width", 60);
    $.post("/user/islogin.do", function (data) {
      if (data.code != "500") {
        $("#user_name").text(data.msg);
        $("#login_li").css({"height": "100%", "width": "120px"});
        $("#loginInfoDiv").css({"width": "100%", "font-size": "11px", "color": "#fff"});
        $("#loginInfoDiv").show();
        $("#loginDiv").hide();
      } else {
        $("#loginInfoDiv").hide();
        $("#loginDiv").show();
      }
    });
    $("#user_name1").click(function () {
      window.location = "/xzsp/query.do";
    });
    $("#login_a").click(function () {
      var localurl = window.location.href;
      // var url = "https://zwdtuser.sh.gov.cn/uc/login/login.jsp?type=2&redirect_uri=" + localurl;
      $.post("/user/getJyhToken.do", function (data) {
        if (data) {
          var url = "https://center1.sheitc.sh.gov.cn/center" +
              "/user/getUser?accessToken=" + data + "&callbackUrl=" + localurl + "&type=2";
          window.location = url;
        }
      });
    });
    $("#logout_btn").click(function () {
      window.location = "/user/pageLogout.do";
    });
    $("#djc_div").click(function () {
      layer.open({
        type: 1,
        title: false,
        shadeClose: true,
        closeBtn: 1,
        shade: false,
        area: ['929px', '615px'],
        content: $("#djc_div_show")
      });
    });
    // autocomplete
    $("#searchKey").autocomplete({
      select: function (event, ui) {
        $("#searchKey").val(ui.item.value);
        $("#searchForm").attr("action", "/cms/ArtiSearch.do");
        $("#searchForm").submit();
      }
    });
    $.post("/cms/keywordsuggest.do", function (data) {
      if (data) {
        $("#searchKey").autocomplete("option", "source", data);
      }
    });
  });
</script>

