<script type="text/javascript">
function DownLoadFile(contentID, title) {
                window.open("/Admin/Upload/FileDownload/?ContentID=" + contentID + "&fileName=" + title);
            }
    $(function () {

        $("#Year").change(function () {

            var value = $(this).children('option:selected').val();

            if (value > 0) {

                $.ajax({

                    url: '/Modules/AjaxIssue',
                    type: 'POST',
                    data: { 'year': value },
                    success: function (result) {
                        var returnData = eval(result);
                        $("#Issue option").remove();
                        for (var i = 0; i < returnData.length; i++) {
                            $("#Issue").append("<option value='" + returnData[i].Issue + "'>No." + returnData[i].Issue + "</option>");
                        }

                    }

                });


            }
        });


    });


        
</script>
