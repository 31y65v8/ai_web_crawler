    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null)
            return unescape(r[2]);
        return null;
    }

    document.getElementById("Year").value = getQueryString("Year") == null ? "" : getQueryString("Year");
    document.getElementById("Issue").value = getQueryString("Issue") == null ? "" : getQueryString("Issue");
    document.getElementById("MenuTree").value = getQueryString("JChannelID") == null ? "" : getQueryString("JChannelID");

    document.getElementById("Title").value = getQueryString("Title") == null ? "标题" : getQueryString("Title");
    document.getElementById("Authors").value = getQueryString("Authors") == null ? "作者姓名" : getQueryString("Authors");
    document.getElementById("WorkUnit").value = getQueryString("WorkUnit") == null ? "作者单位" : getQueryString("WorkUnit");

</script>


