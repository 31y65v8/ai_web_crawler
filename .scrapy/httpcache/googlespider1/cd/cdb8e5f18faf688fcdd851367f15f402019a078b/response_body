    var dataLayer= {};
    var custom_data_1 = '';
    var custom_data_2 = ''; 
    var memberFirmAccountID = '';
    var searchAllSitesAccountID = ''; 
    if(custom_data_1.length > 0){
    	custom_data_1 =sc_country + ';' + sc_language +'_'+ (custom_data_1.toLowerCase()).replace(/ /g,"_");
    }
    if(custom_data_2.length > 0){
