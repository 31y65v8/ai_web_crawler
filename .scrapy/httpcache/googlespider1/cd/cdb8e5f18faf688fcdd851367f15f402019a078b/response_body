var apiCallFunction = function () {

    var cacheBustDate = new Date();
    var dmdburl = "//api.company-target.com/api/v3/ip.json?key=M65mmM3vaL3giOy4cNEWnOAuvqHh83eUMjTyVFvM&x=" +
        cacheBustDate.getMilliseconds();
