        var mt = 0;
        window.onload = function () {
            var topdh = document.getElementById("topdh");
            var mt = topdh.offsetTop;
            window.onscroll = function () {
                var t = document.documentElement.scrollTop || document.body.scrollTop;
                if (t > mt) {
                    topdh.style.position = "fixed";
                    topdh.style.margin = "0 auto";
                    topdh.style.top = "0";
                    topdh.style.width = "100%";
                }
                else {
                    topdh.style.margin = "0px 0";
                    topdh.style.position = "static";
                }
            }
        }
        
    </script>


