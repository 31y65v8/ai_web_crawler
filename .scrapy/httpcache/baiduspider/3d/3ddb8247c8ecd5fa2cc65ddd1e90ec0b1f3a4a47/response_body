    <script>
        $(document).ready(function () {
            // 为所有aios_tabs_items元素绑定hover事件
            $('.aios_tabs_items').hover(
                // 鼠标进入元素时触发的函数
                function () {
                    // 移除所有aios_tabs_items元素上的tabs_active类
                    $('.aios_tabs_items').removeClass('tabs_active');
                    // 给当前悬停的元素添加tabs_active类
                    $(this).addClass('tabs_active');


                    // 获取当前悬停元素的data-flag属性值
                    let flagValue = $(this).data('flag');

                    // 获取aios_tabs_body下所有元素
                    $('.aios_tabs_body > *').each(function () {
                        var elementFlag = $(this).data('flag');
                        if (elementFlag === flagValue) {
                            // 如果元素的data-flag属性值与当前悬停元素的data-flag属性值相同，则显示该元素
                            $(this).show();
                        } else {
                            // 否则，隐藏该元素
                            $(this).hide();
                        }
                    });


                },
                // 鼠标离开元素时触发的函数（如果需要）
                function () {
                    // 这里可以写鼠标离开时的逻辑，如果不需要可以留空
                }
            );
        });
    </script>
</body></html>