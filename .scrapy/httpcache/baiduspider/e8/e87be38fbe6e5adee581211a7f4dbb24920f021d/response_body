	                    bds.util.setContainerWidth(1280);
	                    bds.ready(function(){
	                        $(window).on("resize",function(){
                                var ua = navigator.userAgent.toLowerCase();
                                // 当safari监听onresize事件获取当前可视区窗口with时，获取的宽度不是最后完成时刻的宽度
                                if (/safari/.test(ua)) {
                                    setTimeout(function () {
                                        bds.util.setContainerWidth();
                                    }, 800);
                                } else {
                                    bds.util.setContainerWidth();
                                }
	                            
